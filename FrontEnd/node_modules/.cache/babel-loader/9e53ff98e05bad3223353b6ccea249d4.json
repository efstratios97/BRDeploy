              <span class=\"btn-label\"><b-icon-person-plus-fill /></span>\r\n                </button>\r\n              </div>\r\n              <transition class=\"modal-animation\">\r\n                <modal-view v-if=\"showAddUser\" @close=\"toggleShowAddUser\">\r\n                  <template v-slot:header>ADD USER</template>\r\n                  <template v-slot:body>\r\n                    <add-user @close=\"refreshData\"> </add-user> </template\r\n                ></modal-view>\r\n              </transition>\r\n              <br class=\"br-sub\" />\r\n              <div class=\"col\">\r\n                <h1 class=\"h1_sub\">Users</h1>\r\n                <hr class=\"tablehr\" />\r\n                <user-table :key=\"componentKey\"></user-table>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-4\">\r\n              <h1 class=\"h1_sub\">New Department</h1>\r\n              <div class=\"d-grid\">\r\n                <button\r\n                  class=\"btn btn-primary\"\r\n                  type=\"button\"\r\n                  @click=\"toggleShowAddDep\"\r\n                >\r\n                  <span class=\"btn-label\"><b-icon-plus-circle-fill /></span>\r\n                </button>\r\n              </div>\r\n              <transition class=\"modal-animation\">\r\n                <modal-view v-if=\"showAddDep\" @close=\"toggleShowAddDep\">\r\n                  <template v-slot:header>ADD Department</template>\r\n                  <template v-slot:body>\r\n                    <add-dep @close=\"refreshData\"> </add-dep> </template\r\n                ></modal-view>\r\n              </transition>\r\n              <br />\r\n              <div class=\"col\">\r\n                <h1 class=\"h1_sub\">Departments</h1>\r\n                <hr class=\"tablehr\" />\r\n                <dep-table :key=\"componentKey\"></dep-table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <br />\r\n        <br />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserTable from \"../components/UserTable.vue\";\r\nimport TableDepartments from \"../components/TableDepartments.vue\";\r\nimport AddUser from \"../components/AddUser.vue\";\r\nimport AddDepartment from \"../components/AddDepartment.vue\";\r\nimport Modal from \"../components/Modal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"user-table\": UserTable,\r\n    \"add-user\": AddUser,\r\n    \"add-dep\": AddDepartment,\r\n    \"modal-view\": Modal,\r\n    \"dep-table\": TableDepartments,\r\n  },\r\n  data() {\r\n    this.autenticateSession();\r\n    return {\r\n      showAddUser: false,\r\n      showAddDep: false,\r\n      componentKey: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleShowAddUser() {\r\n      this.showAddUser = !this.showAddUser;\r\n    },\r\n    toggleShowAddDep() {\r\n      this.showAddDep = !this.showAddDep;\r\n    },\r\n    refreshData() {\r\n      this.componentKey += 1;\r\n      if (this.showAddUser === true) {\r\n        this.toggleShowAddUser();\r\n      }\r\n      if (this.showAddDep === true) {\r\n        this.toggleShowAddDep();\r\n      }\r\n    },\r\n    autenticateSession() {\r\n      if (localStorage.loggedUser && localStorage.token) {\r\n        console.log(localStorage.token);\r\n        console.log(localStorage.loggedUser);\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/user/validatetoken?token=\" +\r\n              localStorage.token\r\n          )\r\n          .then(() => {\r\n            this.selected_user = localStorage.loggedUser;\r\n            this.axios\r\n              .get(\"http://127.0.0.1:8081/user/\" + this.selected_user)\r\n              .then((res) => {\r\n                this.$store.state.loggedUser = res.data;\r\n                console.log(this.$store.state.loggedUser);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$router.push(\"/\");\r\n          });\r\n      } else {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}