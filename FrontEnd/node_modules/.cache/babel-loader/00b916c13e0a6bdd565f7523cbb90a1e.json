AL,CAAY,GAAZ,CAAgB;AACd,UAAA,QAAQ,EAAE,MADI;AAEd,UAAA,OAAO,EAAE,wBAFK;AAGd,UAAA,MAAM,EAAE,gCAHM;AAId,UAAA,IAAI,EAAE;AAJQ,SAAhB;AAMF,OAPA,MAOO;AACL,aAAK,cAAL,GAAsB,IAAtB;AACF;;AACA,WAAK,KAAL,CACG,MADH,CAEI,6CACE,MADF,GAEE,GAFF,GAGE,YAAY,CAAC,UALnB,EAOG,IAPH,CAOQ,YAAM;AACV,QAAA,KAAI,CAAC,eAAL;;AACA,YAAI,KAAI,CAAC,cAAL,KAAwB,IAA5B,EAAkC;AAChC,UAAA,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB;AACd,YAAA,QAAQ,EAAE,SADI;AAEd,YAAA,OAAO,EAAE,gCAFK;AAGd,YAAA,MAAM,EAAE,8BAA8B,MAA9B,GAAuC,cAHjC;AAId,YAAA,IAAI,EAAE;AAJQ,WAAhB;AAMF;AACD,OAjBH,EAkBG,KAlBH,CAkBS,YAAM;AACX,QAAA,KAAI,CAAC,iBAAL,GAAyB,KAAzB;;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB;AACd,UAAA,QAAQ,EAAE,OADI;AAEd,UAAA,OAAO,EAAE,kCAFK;AAGd,UAAA,MAAM,EACJ,8BACA,MADA,GAEA,yDANY;AAOd,UAAA,IAAI,EAAE;AAPQ,SAAhB;AASD,OA7BH;AA8BD,KA1CM;AA2CP,IAAA,eA3CO,6BA2CW;AAAA;;AAChB,WAAK,KAAL,CAAW,GAAX,CAAe,mCAAf,EAAoD,IAApD,CAAyD,UAAC,GAAD,EAAS;AAChE,QAAA,MAAI,CAAC,WAAL,GAAmB,GAAG,CAAC,IAAvB;;AACA,YAAI,MAAI,CAAC,WAAL,CAAiB,MAAjB,KAA4B,CAAhC,EAAmC;AACjC,UAAA,MAAI,CAAC,mBAAL,GAA2B,KAA3B;AACF;AACD,OALD;AAMD;AAlDM;AATI,CAAf","sourcesContent":["<template>\r\n  <DataTable\r\n    :value=\"departments\"\r\n    v-model:selection=\"selected_department\"\r\n    dataKey=\"department_id\"\r\n    :paginator=\"true\"\r\n    :rows=\"10\"\r\n    filterDisplay=\"menu\"\r\n    paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\r\n    :rowsPerPageOptions=\"[10, 25, 50]\"\r\n    currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\r\n    responsiveLayout=\"scroll\"\r\n  >\r\n    <template #header>\r\n      <div class=\"p-d-flex p-jc-between p-ai-center\">\r\n        <h5 class=\"p-m-0\">Datasets</h5>\r\n        <span class=\"p-input-icon-left\">\r\n          <i class=\"pi pi-search\" />\r\n          <InputText placeholder=\"Keyword Search\" />\r\n        </span>\r\n      </div>\r\n    </template>\r\n    <template #empty>\r\n      <span v-if=\"departments_loading\">\r\n        Loading Departments. Please wait.\r\n        <ProgressBar mode=\"indeterminate\" />\r\n      </span>\r\n      <span v-else> There are no departmens. </span>\r\n    </template>\r\n    <template #loading>\r\n      Loading Departments. Please wait.\r\n      <ProgressBar mode=\"indeterminate\" />\r\n    </template>\r\n    <Column selectionMode=\"single\" headerStyle=\"width: 3em\"></Column>\r\n    <Column field=\"department_id\" header=\"ID\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column field=\"name\" header=\"Name\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column header=\"Ad hoc Operations\" style=\"min-width: auto\">\r\n      <template #body>\r\n        <div class=\"btn-align-td\">\r\n          <button\r\n            v-on:click=\"deleteDep(this.selected_department.department_id)\"\r\n            class=\"btn btn-secondary\"\r\n            v-tooltip=\"'Select Department first'\"\r\n          >\r\n            <b-icon-trash-fill />\r\n          </button>\r\n        </div>\r\n      </template>\r\n    </Column>\r\n  </DataTable>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      departments: this.listDepartments(),\r\n      selected_department: \"\",\r\n      departments_loading: true,\r\n      dep_operations: false,\r\n    };\r\n  },\r\n  methods: {\r\n    deleteDep(dep_id) {\r\n      if (dep_id === undefined) {\r\n        this.$toast.add({\r\n          severity: \"warn\",\r\n          summary: \"No Department selected\",\r\n          detail: \"Please select Department first\",\r\n          life: 3000,\r\n        });\r\n      } else {\r\n        this.dep_operations = true;\r\n      }\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_department/\" +\r\n            dep_id +\r\n            \"/\" +\r\n            localStorage.loggedUser\r\n        )\r\n        .then(() => {\r\n          this.listDepartments();\r\n          if (this.dep_operations === true) {\r\n            this.$toast.add({\r\n              severity: \"success\",\r\n              summary: \"Department Deletion Successful\",\r\n              detail: \"The selected Department: \" + dep_id + \" was deleted\",\r\n              life: 3000,\r\n            });\r\n          }\r\n        })\r\n        .catch(() => {\r\n          this.dataset_operating = false;\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Department Deletion Unsuccessful\",\r\n            detail:\r\n              \"The selected Department: \" +\r\n              dep_id +\r\n              \" could not be deleted!\\n Check if you have admin rights\",\r\n            life: 3000,\r\n          });\r\n        });\r\n    },\r\n    listDepartments() {\r\n      this.axios.get(\"http://127.0.0.1:8081/departments\").then((res) => {\r\n        this.departments = res.data;\r\n        if (this.departments.length === 0) {\r\n          this.departments_loading = false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: #0c8cd3;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}