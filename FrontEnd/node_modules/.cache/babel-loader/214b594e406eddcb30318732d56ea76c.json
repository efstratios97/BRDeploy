,MADI;AAEd,UAAA,OAAO,EAAE,qBAFK;AAGd,UAAA,MAAM,EAAE,6BAHM;AAId,UAAA,IAAI,EAAE;AAJQ,SAAhB;AAMF,OAPA,MAOO;AACL,aAAK,iBAAL,GAAyB,IAAzB;AACF;;AACA,WAAK,KAAL,CACG,MADH,CAEI,0CACE,UADF,GAEE,GAFF,GAGE,WALN,EAOG,IAPH,CAOQ,YAAM;AACV,QAAA,KAAI,CAAC,YAAL;;AACA,QAAA,KAAI,CAAC,gBAAL,GAAwB,EAAxB;AACA,QAAA,KAAI,CAAC,iBAAL,GAAyB,KAAzB;;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB;AACd,UAAA,QAAQ,EAAE,SADI;AAEd,UAAA,OAAO,EAAE,6BAFK;AAGd,UAAA,MAAM,EAAE,kCAHM;AAId,UAAA,IAAI,EAAE;AAJQ,SAAhB;AAMD,OAjBH,EAkBG,KAlBH,CAkBS,YAAM;AACX,QAAA,KAAI,CAAC,iBAAL,GAAyB,KAAzB;;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,GAAZ,CAAgB;AACd,UAAA,QAAQ,EAAE,OADI;AAEd,UAAA,OAAO,EAAE,+BAFK;AAGd,UAAA,MAAM,EAAE,2CAHM;AAId,UAAA,IAAI,EAAE;AAJQ,SAAhB;AAMD,OA1BH;AA2BD,KA9FM;AA+FP,IAAA,WA/FO,uBA+FK,UA/FL,EA+FiB,YA/FjB,EA+F+B;AACpC,UAAI,UAAS,KAAM,SAAnB,EAA8B;AAC5B,aAAK,MAAL,CAAY,GAAZ,CAAgB;AACd,UAAA,QAAQ,EAAE,MADI;AAEd,UAAA,OAAO,EAAE,qBAFK;AAGd,UAAA,MAAM,EAAE,6BAHM;AAId,UAAA,IAAI,EAAE;AAJQ,SAAhB;AAMF;;AACA,WAAK,KAAL,CAAW,MAAX,EAAmB,UAAnB,EAA+B,YAA/B;AACD,KAzGM;AA0GP,IAAA,YA1GO,0BA0GQ;AAAA;;AACb,WAAK,KAAL,CACG,GADH,CACO,wCAAwC,YAAY,CAAC,UAD5D,EAEG,IAFH,CAEQ,UAAC,GAAD,EAAS;AACb,QAAA,MAAI,CAAC,QAAL,GAAgB,MAAI,CAAC,kBAAL,CAAwB,GAAG,CAAC,IAAJ,CAAS,IAAjC,CAAhB;;AACA,YAAI,MAAI,CAAC,QAAL,CAAc,MAAd,KAAyB,CAA7B,EAAgC;AAC9B,UAAA,MAAI,CAAC,eAAL,GAAuB,KAAvB;AACF;AACD,OAPH;AAQD,KAnHM;AAoHP,IAAA,SApHO,uBAoHK;AAAA;;AACV,WAAK,KAAL,CAAW,GAAX,CAAe,6BAAf,EAA8C,IAA9C,CAAmD,UAAC,GAAD,EAAS;AAC1D,QAAA,MAAI,CAAC,KAAL,GAAa,GAAG,CAAC,IAAjB;AACD,OAFD;AAGD;AAxHM;AAVI,CAAf","sourcesContent":["<template>\r\n  <DataTable\r\n    :value=\"datasets\"\r\n    v-model:selection=\"selected_dataset\"\r\n    dataKey=\"dataset_id\"\r\n    :paginator=\"true\"\r\n    :rows=\"10\"\r\n    filterDisplay=\"menu\"\r\n    paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\r\n    :rowsPerPageOptions=\"[10, 25, 50]\"\r\n    currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\r\n    responsiveLayout=\"scroll\"\r\n  >\r\n    <template #header>\r\n      <div class=\"p-d-flex p-jc-between p-ai-center\">\r\n        <h5 class=\"p-m-0\">Datasets</h5>\r\n        <span class=\"p-input-icon-left\">\r\n          <i class=\"pi pi-search\" />\r\n          <InputText placeholder=\"Keyword Search\" />\r\n        </span>\r\n      </div>\r\n    </template>\r\n    <template #empty>\r\n      <span v-if=\"dataset_loading\">\r\n        Loading Datasets. Please wait.\r\n        <ProgressBar mode=\"indeterminate\" />\r\n      </span>\r\n      <span v-else> There are no datasets. </span>\r\n    </template>\r\n    <template #loading>\r\n      Loading Datasets. Please wait. <ProgressBar mode=\"indeterminate\"\r\n    /></template>\r\n    <Column selectionMode=\"single\" headerStyle=\"width: 3em\"></Column>\r\n    <Column field=\"dataset_id\" header=\"ID\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column field=\"name\" header=\"Name\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"description\"\r\n      header=\"Description\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column field=\"cleaned\" header=\"Cleaned\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column field=\"owner\" header=\"Owner\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"storage_type\"\r\n      header=\"Storage Location\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column\r\n      field=\"access_user_list\"\r\n      header=\"Access Users\"\r\n      headerStyle=\"min-width: 4rem; text-align: center\"\r\n      bodyStyle=\"text-align: center; overflow: visible; color:black\"\r\n    >\r\n      <template #body=\"slotProps\">\r\n        <Dropdown\r\n          :options=\"slotProps.data.access_user_list\"\r\n          placeholder=\"Users\"\r\n        />\r\n      </template>\r\n    </Column>\r\n    <Column\r\n      field=\"access_business_unit_list\"\r\n      header=\"Access BUs\"\r\n      style=\"min-width: auto\"\r\n    >\r\n      <template #body=\"slotProps\">\r\n        <Dropdown\r\n          :options=\"slotProps.data.access_business_unit_list\"\r\n          placeholder=\"BUs\"\r\n        />\r\n      </template>\r\n    </Column>\r\n    <!-- <Column\r\n      field=\"hash_of_dataset\"\r\n      header=\"Hash of Dataset\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column> -->\r\n    <Column header=\"Ad hoc Operations\" style=\"min-width: auto\">\r\n      <template #body>\r\n        <div class=\"btn-align-td\">\r\n          <button\r\n            v-on:click=\"deleteSelected()\"\r\n            class=\"btn btn-secondary\"\r\n            v-tooltip=\"'Select Dataset first'\"\r\n          >\r\n            <b-icon-trash-fill />\r\n          </button>\r\n        </div>\r\n        <div class=\"btn-align-td\">\r\n          <button\r\n            v-on:click=\"emitSelected()\"\r\n            class=\"btn btn-secondary\"\r\n            v-tooltip=\"'Select Dataset first'\"\r\n          >\r\n            <b-icon-eye-fill></b-icon-eye-fill>\r\n          </button>\r\n        </div>\r\n        <ProgressBar mode=\"indeterminate\" v-if=\"dataset_operating\" />\r\n      </template>\r\n    </Column>\r\n  </DataTable>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: this.listUsers(),\r\n      datasets: this.listDatasets(),\r\n      selected_dataset: \"\",\r\n      dataset_loading: true,\r\n      dataset_operating: false,\r\n    };\r\n  },\r\n  methods: {\r\n    deleteSelected() {\r\n      this.deleteDataSet(\r\n        this.selected_dataset.dataset_id,\r\n        this.selected_dataset.storage_type\r\n      );\r\n    },\r\n    emitSelected() {\r\n      this.emitDataset(\r\n        this.selected_dataset.dataset_id,\r\n        this.selected_dataset.storage_type\r\n      );\r\n    },\r\n    normalize_datasets(datasets) {\r\n      for (let index = 0; index < datasets.length; index++) {\r\n        for (var key in datasets[index]) {\r\n          if (key === \"owner\") {\r\n            datasets[index][key] = this.enumOwner(datasets[index][key]);\r\n          }\r\n          if (key === \"user\") {\r\n            datasets[index][key] = this.enumUser(datasets[index][key]);\r\n          }\r\n          if (key === \"access_user_list\") {\r\n            var tmp_list = [];\r\n            for (let i = 0; i < datasets[index][key].length; i++) {\r\n              tmp_list.push(this.enumUser(datasets[index][key][i]));\r\n            }\r\n            datasets[index][key] = tmp_list;\r\n          }\r\n          if (key === \"access_business_unit_list\") {\r\n            tmp_list = [];\r\n            for (let i = 0; i < datasets[index][key].length; i++) {\r\n              tmp_list.push(this.enumUser(datasets[index][key][i]));\r\n            }\r\n            datasets[index][key] = tmp_list;\r\n          }\r\n        }\r\n      }\r\n      return datasets;\r\n    },\r\n    enumOwner(owner) {\r\n      for (let i = 0; i < this.users.length; i++) {\r\n        if (this.users[i][\"user_id\"] === owner) {\r\n          return this.users[i][\"email\"];\r\n        }\r\n      }\r\n      return owner;\r\n    },\r\n    enumUser(user) {\r\n      for (let i = 0; i < this.users.length; i++) {\r\n        if (this.users[i][\"user_id\"] === user) {\r\n          return this.users[i][\"email\"];\r\n        }\r\n      }\r\n      return user;\r\n    },\r\n    deleteDataSet(dataset_id, storageType) {\r\n      if (dataset_id === undefined) {\r\n        this.$toast.add({\r\n          severity: \"warn\",\r\n          summary: \"No Dataset selected\",\r\n          detail: \"Please select Dataset first\",\r\n          life: 3000,\r\n        });\r\n      } else {\r\n        this.dataset_operating = true;\r\n      }\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_dataset/\" +\r\n            dataset_id +\r\n            \"/\" +\r\n            storageType\r\n        )\r\n        .then(() => {\r\n          this.listDatasets();\r\n          this.selected_dataset = \"\";\r\n          this.dataset_operating = false;\r\n          this.$toast.add({\r\n            severity: \"success\",\r\n            summary: \"Dataset Deletion Successful\",\r\n            detail: \"The selected Dataset was deleted\",\r\n            life: 3000,\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.dataset_operating = false;\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Dataset Deletion Unsuccessful\",\r\n            detail: \"The selected Dataset could not be deleted\",\r\n            life: 3000,\r\n          });\r\n        });\r\n    },\r\n    emitDataset(dataset_id, storage_type) {\r\n      if (dataset_id === undefined) {\r\n        this.$toast.add({\r\n          severity: \"warn\",\r\n          summary: \"No Dataset selected\",\r\n          detail: \"Please select Dataset first\",\r\n          life: 3000,\r\n        });\r\n      }\r\n      this.$emit(\"view\", dataset_id, storage_type);\r\n    },\r\n    listDatasets() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/get_datasets/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          this.datasets = this.normalize_datasets(res.data.data);\r\n          if (this.datasets.length === 0) {\r\n            this.dataset_loading = false;\r\n          }\r\n        });\r\n    },\r\n    listUsers() {\r\n      this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.btn-align-td {\r\n  display: inline-block;\r\n  margin: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}