M,EAAE,IAMT;AALA,IAAA,WAAW,EAAC,SAKZ;AAJA,IAAA,WAAW,EAAC,6BAIZ;AAHA,IAAA,iBAAiB,EAAC,cAGlB;AAFA,IAAA,KAAK,EAAC,oBAEN;AADC,IAAA,SAAS,EAAE;AACZ,GATF,E,IAAA,E;;AAAA,I,yBAAA,CAUI,CAdN,CAeI,CAhBN,CAiBI,CAlBN,CAiCK,EAdL,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARJ,YAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANJ,YAAA,CAKE,iBALF,EAKE;AAJA,IAAA,KAAK,EAAC,eAIN;AAHA,IAAA,IAAI,EAAC,mBAGL;AAFA,IAAA,OAAO,EAAC,QAER;AADC,IAAA,OAAK,EAAE,QAAA,CAAA;AACR,GALF,E,IAAA,E;;AAAA,I,WAAA,CAMI,CAPN,CAQI,CATN,CAcK,EAJM,KAAA,CAAA,S,kBAAX,YAAA,CAGM,KAHN,EAGM,WAHN,EAGM,CAFJ,WAEI,EADoC,KAAA,CAAA,S,kBAAxC,YAAA,CAAqD,sBAArD,EAAqD;UAAA;AAAxC,IAAA,IAAI,EAAC;AAAmC,GAArD,C,qCACI,CAHN,C,qCAIK,CA3HP,CA4HI,CA7HN,C","sourcesContent":["<template>\r\n  <div>\r\n    <form>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-12 p-md-12\">\r\n          <div class=\"p-inputgroup\">\r\n            <span class=\"p-inputgroup-addon\">\r\n              <i class=\"pi pi-file\"></i>\r\n            </span>\r\n            <InputText\r\n              placeholder=\"Aspect's Name\"\r\n              v-model=\"name\"\r\n              class=\"inputfield w-full\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-12 p-md-12\">\r\n          <Textarea\r\n            placeholder=\"Description\"\r\n            v-model=\"description\"\r\n            :autoResize=\"true\"\r\n            rows=\"5\"\r\n            cols=\"30\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-12 p-md-12\">\r\n          <div class=\"p-inputgroup\">\r\n            <span class=\"p-inputgroup-addon\">\r\n              <i class=\"pi pi-file\"></i>\r\n            </span>\r\n            <MultiSelect\r\n              v-model=\"selected_dataset_labels\"\r\n              :options=\"labels\"\r\n              :filter=\"true\"\r\n              optionLabel=\"label\"\r\n              placeholder=\"Assign applicable Labels\"\r\n              filterPlaceholder=\"Find Labels\"\r\n              :showClear=\"true\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-for=\"number in number_components\" :key=\"number\">\r\n        <add-aspect-helper\r\n          :selected_dataset_id=\"selected_dataset_id\"\r\n          :selected_dataset_label=\"selected_dataset_label\"\r\n          :id=\"id\"\r\n          @selected-raw-component-operation-type=\"\r\n            add_to_raw_components_operation_types($event)\r\n          \"\r\n        ></add-aspect-helper>\r\n      </div>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <Button\r\n            label=\"Add Further Component\"\r\n            icon=\"pi pi-plus-circle\"\r\n            iconPos=\"center\"\r\n            class=\"p-button-success\"\r\n            @click=\"add_further_component\"\r\n          />\r\n        </div>\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <Button\r\n            label=\"Remove Component\"\r\n            icon=\"pi pi-minus-circle\"\r\n            iconPos=\"center\"\r\n            class=\"p-button-danger\"\r\n            @click=\"remove_further_component\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-align-center\">\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <h3>Assign Scale Size:</h3>\r\n        </div>\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <Knob v-model=\"selected_skala_size\" :min=\"0\" :max=\"10\" />\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-align-center\">\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <h3>Assign Threshold:</h3>\r\n        </div>\r\n        <div class=\"p-col-6 p-md-6\">\r\n          <Knob v-model=\"selected_threshold\" :min=\"0\" :max=\"10\" />\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-12 p-md-12\">\r\n          <div class=\"p-inputgroup\">\r\n            <span class=\"p-inputgroup-addon\">\r\n              <b-icon-calculator />\r\n            </span>\r\n            <Dropdown\r\n              v-model=\"selected_formula\"\r\n              :options=\"formulas\"\r\n              :filter=\"true\"\r\n              optionLabel=\"formula\"\r\n              placeholder=\"Assign Formula to an Aspect\"\r\n              filterPlaceholder=\"Find Formula\"\r\n              class=\"multiselect-custom\"\r\n              :showClear=\"true\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-grid p-fluid\">\r\n        <div class=\"p-col-12 p-md-12\">\r\n          <Button\r\n            label=\"Create Aspect\"\r\n            icon=\"pi pi-plus-circle\"\r\n            iconPos=\"center\"\r\n            @click=\"create_aspect\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <div v-if=\"submitted\">\r\n        <br />\r\n        <ProgressBar mode=\"indeterminate\" v-if=\"submitted\" />\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport KPIApplicabilityGauge from \"../Plots/KPIApplicabilityGauge.vue\";\r\nimport AddAspectHelper from \"../HelperComponents/AddAspectHelper.vue\";\r\nexport default {\r\n  components: {\r\n    \"kpi-applicability-gauge\": KPIApplicabilityGauge,\r\n    \"add-aspect-helper\": AddAspectHelper,\r\n  },\r\n  props: [\"selected_dataset_id\", \"selected_dataset_label\"],\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      description: \"\",\r\n      datasets: this.getDatasetOptions(),\r\n      labels: this.getLabelsOptions(),\r\n      formulas: this.getFormulas(),\r\n      selected_dataset_labels: \"\",\r\n      selected_raw_components_operation_types: [],\r\n      selected_skala_size: 10,\r\n      selected_threshold: 0,\r\n      selected_weight: 0,\r\n      id: \"gauge_\",\r\n      selected_formula: \"\",\r\n      selected_kpi_family: \"\",\r\n      number_components: 1,\r\n      formData: \"\",\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    add_to_raw_components_operation_types(raw_component_operation_types) {\r\n      console.log(raw_component_operation_types);\r\n      var obj_to_push = {};\r\n      obj_to_push[raw_component_operation_types[0]] =\r\n        raw_component_operation_types[1];\r\n      if (\r\n        raw_component_operation_types[1] !== undefined &&\r\n        raw_component_operation_types[0] !== undefined\r\n      ) {\r\n        this.selected_raw_components_operation_types.push(obj_to_push);\r\n      }\r\n      console.log(this.selected_raw_components_operation_types);\r\n    },\r\n    add_further_component() {\r\n      if (\r\n        this.selected_type === \"\" &&\r\n        this.selected_raw_component === \"\" &&\r\n        this.number_components !== 0\r\n      ) {\r\n        this.$toast.add({\r\n          severity: \"warn\",\r\n          summary: \"No Component and/or Category chosen\",\r\n          detail: \"Please select first the Component & Category\",\r\n          life: 4000,\r\n        });\r\n      } else {\r\n        this.number_components = this.number_components + 1;\r\n        this.id = this.id + this.number_components;\r\n        console.log(this.selected_raw_components_operation_types);\r\n      }\r\n    },\r\n    remove_further_component() {\r\n      if (this.number_components > 0) {\r\n        this.number_components = this.number_components - 1;\r\n      }\r\n      this.selected_raw_components_operation_types.pop();\r\n      console.log(this.selected_raw_components_operation_types);\r\n    },\r\n    getDatasetOptions() {\r\n      this.$axios\r\n        .get(\"/get_datasets_only_name/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          var datasets_tmp = [];\r\n          for (let index = 0; index < res.data.data.length; index++) {\r\n            datasets_tmp.push({\r\n              dataset:\r\n                \"NAME: \" +\r\n                res.data.data[index][\"name\"] +\r\n                \"   |   \" +\r\n                \"ID: \" +\r\n                res.data.data[index][\"dataset_id\"],\r\n            });\r\n          }\r\n          this.datasets = datasets_tmp;\r\n        });\r\n    },\r\n    getLabelsOptions() {\r\n      this.$axios.get(\"/get_all_labels\").then((res) => {\r\n        var labels_tmp = [];\r\n        for (let index = 0; index < res.data.data.length; index++) {\r\n          labels_tmp.push({\r\n            label:\r\n              \"NAME: \" +\r\n              res.data.data[index][\"name\"] +\r\n              \"   |   \" +\r\n              \"ID: \" +\r\n              res.data.data[index][\"label_id\"],\r\n          });\r\n        }\r\n        this.labels = labels_tmp;\r\n      });\r\n    },\r\n    getFormulas() {\r\n      this.$axios.get(\"/get_formulas\").then((res) => {\r\n        var formulas_tmp = [];\r\n        for (let index = 0; index < res.data.data.length; index++) {\r\n          formulas_tmp.push({\r\n            formula: res.data.data[index][\"name\"],\r\n          });\r\n        }\r\n        this.formulas = formulas_tmp;\r\n      });\r\n    },\r\n    create_aspect() {\r\n      this.submitted = true;\r\n      this.formData = new FormData();\r\n      this.formData.append(\"name\", this.name);\r\n      this.formData.append(\"description\", this.description);\r\n      this.formData.append(\r\n        \"raw_components_from_dataset\",\r\n        this.selected_raw_components_operation_types\r\n      );\r\n      this.formData.append(\"skala_size\", this.selected_skala_size);\r\n      this.formData.append(\"weight\", this.selected_weight);\r\n      this.formData.append(\"threshold\", this.selected_threshold);\r\n      this.formData.append(\"operation_type\", this.selected_operation_type);\r\n      this.formData.append(\"formula\", this.selected_formula);\r\n      this.formData.append(\"dataset_id\", this.selected_dataset_id);\r\n      this.formData.append(\"dataset_labels\", this.selected_dataset_labels);\r\n      this.formData.append(\"kpi_family\", this.selected_kpi_family);\r\n      this.$axios\r\n        .post(\"/create_aspect\", this.formData)\r\n        .then(() => {\r\n          this.$toast.add({\r\n            severity: \"success\",\r\n            summary: \"Aspect Creation Successful\",\r\n            detail: \"The Aspect was created\",\r\n            life: 3000,\r\n          });\r\n          this.close();\r\n        })\r\n        .catch(() => {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Aspect Creation Unsuccessful\",\r\n            detail: \"The Aspect could not be created\",\r\n            life: 5000,\r\n          });\r\n          this.submitted = false;\r\n        });\r\n    },\r\n    close() {\r\n      this.$emit(\"close\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.grouped_inputs {\r\n  text-align: center;\r\n  margin: 5px;\r\n}\r\n\r\nlabel {\r\n  margin-top: 5px;\r\n  display: block;\r\n}\r\n\r\n.p-checkbox-icon {\r\n  margin: 5px;\r\n}\r\n\r\n.p-field-checkbox {\r\n  margin-top: 6px;\r\n}\r\n\r\n.p-grid {\r\n  margin-top: 15px;\r\n}\r\n\r\n.visuallyhidden {\r\n  border: 0;\r\n  clip: rect(0 0 0 0);\r\n  height: 1px;\r\n  margin: -1px;\r\n  overflow: hidden;\r\n  padding: 0;\r\n  position: absolute;\r\n  width: 1px;\r\n}\r\n\r\nbody {\r\n  font-family: \"Open Sans\", sans-serif;\r\n  color: #1a1a1a;\r\n  background-color: #f0f0f0;\r\n}\r\n\r\nh2 {\r\n  color: #111;\r\n  font-family: \"Open Sans Condensed\", sans-serif;\r\n  font-size: 28x;\r\n  font-weight: 700;\r\n  line-height: 28px;\r\n  margin: 0 0 5px;\r\n  padding: 0 5px;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n}\r\n\r\nh1 {\r\n  color: #111;\r\n  font-family: \"Open Sans Condensed\", sans-serif;\r\n  font-size: 30x;\r\n  font-weight: 700;\r\n  line-height: 35px;\r\n  margin: 0 0 5px;\r\n  padding: 0 5px;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.button {\r\n  color: #ffffff;\r\n  background-color: #24cf5f;\r\n  margin