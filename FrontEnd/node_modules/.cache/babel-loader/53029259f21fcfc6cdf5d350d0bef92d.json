,MAAM,EAAA,QAAA,CACf;AAAA,aAAiC,CAAjC,WAAiC,CAAjC;AAAA,KADe,C;;;;GADnB,E;;AAAA,I,cAAA,CAKI,CAPN,CAcI,EANJ,YAAA,CAKE,iBALF,EAKE;AAJA,IAAA,KAAK,EAAC,QAIN;AAHA,IAAA,IAAI,EAAC,aAGL;AAFA,IAAA,OAAO,EAAC,OAER;AADC,IAAA,OAAK,EAAE,QAAA,CAAA;AACR,GALF,E,IAAA,E;;AAAA,I,WAAA,CAMI,CAzCN,CA0CK,CA3CP,CA4CI,CA7CN,C","sourcesContent":["<template>\r\n  <div>\r\n    <form>\r\n      <div class=\"p-fluid\">\r\n        <div class=\"p-field p-col-5 p-md-5\">\r\n          <label for=\"old_pw\">Please Enter your current Password</label>\r\n          <Password v-model=\"old_password\" :feedback=\"false\">\r\n            <template #header>\r\n              <h6>Enter your current Password</h6>\r\n            </template>\r\n          </Password>\r\n        </div>\r\n        <div class=\"p-field p-col-5 p-sm-5\">\r\n          <label for=\"new_pw1\">Please Enter your new Password</label>\r\n          <Password v-model=\"new_password_1\">\r\n            <template #header>\r\n              <h6>Enter your new Password</h6>\r\n            </template>\r\n            <template #footer>\r\n              <Divider />\r\n              <p class=\"p-mt-2\">Suggestions</p>\r\n              <ul class=\"p-pl-2 p-ml-2 p-mt-0\" style=\"line-height: 1.5\">\r\n                <li>At least one lowercase</li>\r\n                <li>At least one uppercase</li>\r\n                <li>At least one numeric</li>\r\n                <li>Minimum 8 characters</li>\r\n              </ul>\r\n            </template>\r\n          </Password>\r\n        </div>\r\n        <div class=\"p-field p-col-5 p-sm-5\">\r\n          <label for=\"new_pw2\">Please Re-enter your new Password</label>\r\n          <Password v-model=\"new_password_2\" :feedback=\"false\">\r\n            <template #header>\r\n              <h6>Repeat your new Password</h6>\r\n            </template>\r\n          </Password>\r\n        </div>\r\n        <Button\r\n          label=\"Submit\"\r\n          icon=\"pi pi-check\"\r\n          iconPos=\"right\"\r\n          @click=\"updatePassword\"\r\n        />\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: this.getUserMail(),\r\n      old_password: \"\",\r\n      new_password_1: \"\",\r\n      new_password_2: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getUserMail() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/user/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          this.email = res.data.email;\r\n        });\r\n    },\r\n    updatePassword() {\r\n      this.axios\r\n        .get(\r\n          \"http://127.0.0.1:8081/user/auth?email=\" +\r\n            this.email +\r\n            \"&passwd=\" +\r\n            this.old_password\r\n        )\r\n        .then(() => {\r\n          if (this.new_password_2 === this.new_password_1) {\r\n            this.axios;\r\n            var password = {\r\n              old_password: this.old_password,\r\n              new_password: this.new_password_1,\r\n            };\r\n            const jsonString = JSON.stringify(password);\r\n            console.log(jsonString);\r\n            this.axios\r\n              .post(\r\n                \"http://127.0.0.1:8081/change_password/\" +\r\n                  localStorage.loggedUser,\r\n                jsonString\r\n              )\r\n              .then((res) => {\r\n                this.$emit(\"close\");\r\n                console.log(res.data);\r\n              });\r\n            this.$toast.add({\r\n              severity: \"success\",\r\n              summary: \"Successfully updated Password\",\r\n              detail: \"You successfully updated your password\",\r\n              life: 3000,\r\n            });\r\n          } else {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"No Password Matching\",\r\n              detail: \"The entered passwords do not match\",\r\n              life: 3000,\r\n            });\r\n          }\r\n        })\r\n        .catch(() => {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Wrong Password\",\r\n            detail: \"The entered password is wrong\",\r\n            life: 3000,\r\n          });\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}