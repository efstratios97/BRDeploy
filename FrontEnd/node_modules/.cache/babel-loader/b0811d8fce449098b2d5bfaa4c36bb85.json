C,MAAvC,GAAgD,GAAhD,GAAsD,WAF1D,EAIG,IAJH,CAIQ,YAAM,CACV;AACD,OANH;AAOD,KAVM;AAYP,IAAA,SAZO,uBAYK;AACV,UAAI,KAAK,IAAL,CAAU,KAAV,KAAoB,CAAxB,EAA2B;AACzB,eAAO,iBAAP;AACF,OAFA,MAEO;AACL,eAAO,cAAP;AACF;AACD,KAlBM;AAmBP;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,OA1BO,qBA0BG;AACR,WAAK,KAAL,CACG,GADH,CACO,gCAAgC,YAAY,CAAC,UADpD,EAEG,IAFH,CAEQ,UAAC,GAAD,EAAS;AACb,QAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAhB;AACA,eAAO,GAAG,CAAC,IAAX;AACD,OALH;AAMD;AAjCM;AATI,CAAf","sourcesContent":["<template>\r\n  <div>\r\n    <Button\r\n      label=\"Submit\"\r\n      icon=\"pi pi-check\"\r\n      iconPos=\"right\"\r\n      @click=\"getUser\"\r\n    />\r\n    <Card>\r\n      <template v-slot:title> Hello {{ this.user.first_name }}</template>\r\n      <template v-slot:subtitle>\r\n        Here you can see and manage your profile\r\n      </template>\r\n      <template v-slot:content>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"firstname\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Firstname:\r\n          </label>\r\n          <div class=\"p-col\">\r\n            {{ this.user.first_name }}\r\n          </div>\r\n        </div>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"lastname\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Lastname:\r\n          </label>\r\n          <div class=\"p-col\">\r\n            {{ this.user.last_name }}\r\n          </div>\r\n        </div>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"lastname\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Email:\r\n          </label>\r\n          <div class=\"p-col\">\r\n            {{ this.user.email }}\r\n          </div>\r\n        </div>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"department\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Department:\r\n          </label>\r\n          <div class=\"p-col\">\r\n            {{ this.user.business_unit }}\r\n          </div>\r\n        </div>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"rights\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Rights:\r\n          </label>\r\n          <div class=\"p-col\">\r\n            {{ this.enumAdmin() }}\r\n          </div>\r\n        </div>\r\n        <div class=\"p-field p-grid\">\r\n          <label for=\"password\" class=\"p-col-fixed\" style=\"width: 100px\"\r\n            >Change Password:\r\n          </label>\r\n          <Password v-model=\"password\" toggleMask></Password>\r\n        </div>\r\n      </template>\r\n    </Card>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      selected_user: \"\",\r\n      password: \"\",\r\n      showUser: false,\r\n      user: this.getUser(),\r\n    };\r\n  },\r\n  methods: {\r\n    deleteUser(userid) {\r\n      const issued_user = localStorage.loggedUser;\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_user/\" + userid + \"/\" + issued_user\r\n        )\r\n        .then(() => {\r\n          //   this.logout();\r\n        });\r\n    },\r\n\r\n    enumAdmin() {\r\n      if (this.user.admin === 0) {\r\n        return \"No Admin Rights\";\r\n      } else {\r\n        return \"Admin Rights\";\r\n      }\r\n    },\r\n    // logout() {\r\n    //   delete localStorage.logged_institution;\r\n    //   delete localStorage.token;\r\n    //   this.$store.state.logged_institution = null;\r\n    //   this.$store.state.token = null;\r\n    //   this.autenticaSessao();\r\n    // },\r\n    getUser() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/user/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          return res.data;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.p-password input {\r\n  width: 15rem;\r\n}\r\n</style>"],"sourceRoot":""}]}