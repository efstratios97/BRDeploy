s://img.br.de/19f53b6f-8b81-451a-8012-947d7d80cfb3.png?w=800&h=450);\r\n        background-attachment: fixed;\r\n        background-position: center center;\r\n        background-size: cover;\r\n      \"\r\n    >\r\n      <div class=\"overlay\">\r\n        <div class=\"container-fluid\">\r\n          <div class=\"row\">\r\n            <div class=\"col-4\">\r\n              <h1 class=\"h1_sub\">Create Cleanser</h1>\r\n              <div class=\"d-grid\">\r\n                <button\r\n                  class=\"btn btn-primary\"\r\n                  type=\"button\"\r\n                  @click=\"toggleShowAddCleanser\"\r\n                >\r\n                  <span class=\"btn-label\"\r\n                    ><b-icon-file-earmark-plus-fill\r\n                  /></span>\r\n                </button>\r\n              </div>\r\n              <transition class=\"modal-animation\">\r\n                <modal-view\r\n                  v-if=\"showAddCleanser\"\r\n                  @close=\"toggleShowAddCleanser\"\r\n                >\r\n                  <template v-slot:header>ADD Cleanser</template>\r\n                  <template v-slot:body>\r\n                    <add-cleanser @close=\"refreshData\">\r\n                    </add-cleanser> </template\r\n                ></modal-view>\r\n              </transition>\r\n            </div>\r\n            <div class=\"col-4\">\r\n              <h1 class=\"h1_sub\">Update Cleansers</h1>\r\n              <div class=\"d-grid\">\r\n                <button\r\n                  class=\"btn btn-primary\"\r\n                  type=\"button\"\r\n                  @click=\"\r\n                    toggleShowUpdateCleanser();\r\n                    updateCleansers();\r\n                  \"\r\n                >\r\n                  <span class=\"btn-label\"><b-icon-gear-fill /></span>\r\n                </button>\r\n              </div>\r\n              <transition class=\"modal-animation\">\r\n                <modal-view\r\n                  v-if=\"showUpdateCleanser\"\r\n                  @close=\"toggleShowUpdateCleanser\"\r\n                >\r\n                  <template v-slot:header>Update Cleanser</template>\r\n                  <template v-slot:body>\r\n                    <h1>Please Wait While Updating</h1>\r\n                    <h1>This can take some minutes</h1>\r\n                  </template>\r\n                </modal-view>\r\n              </transition>\r\n            </div>\r\n            <div class=\"col-4\">\r\n              <h1 class=\"h1_sub\">Apply Cleansers</h1>\r\n              <div class=\"d-grid\">\r\n                <button\r\n                  class=\"btn btn-primary\"\r\n                  type=\"button\"\r\n                  @click=\"toggleShowApplyCleanser()\"\r\n                >\r\n                  <span class=\"btn-label\"><b-icon-filter-circle-fill /></span>\r\n                </button>\r\n              </div>\r\n              <modal-view\r\n                v-if=\"showApplyCleanser\"\r\n                @close=\"toggleShowApplyCleanser()\"\r\n              >\r\n                <template v-slot:header>Apply Cleanser</template>\r\n                <template v-slot:body>\r\n                  <add-cleaned-data-set\r\n                    @close=\"refreshData()\"\r\n                  ></add-cleaned-data-set>\r\n                </template>\r\n              </modal-view>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <br class=\"br-sub\" />\r\n        <div class=\"container-fluid\">\r\n          <div class=\"row\">\r\n            <div class=\"col-8\">\r\n              <h1 class=\"h1_sub\">Cleansers</h1>\r\n              <hr class=\"tablehr\" />\r\n              <cleanser-table :key=\"componentKey\"></cleanser-table>\r\n            </div>\r\n            <div class=\"col-4\">\r\n              <h1 class=\"h1_sub\">Datasets</h1>\r\n              <hr class=\"tablehr\" />\r\n              <data-set-table\r\n                :key=\"componentKey\"\r\n                @view=\"viewDataset\"\r\n              ></data-set-table>\r\n              <transition class=\"modal-animation\">\r\n                <modal-view\r\n                  v-if=\"showAddTableData\"\r\n                  @close=\"toggleShowAddTableData\"\r\n                >\r\n                  <template v-slot:header>\r\n                    <h1>{{ this.selected_dataset_id }}</h1></template\r\n                  >\r\n                  <template v-slot:body>\r\n                    <data-set-complete-table\r\n                      :dataset_id=\"this.selected_dataset_id\"\r\n                      :storage_type=\"this.selected_storage_type\"\r\n                    >\r\n                    </data-set-complete-table>\r\n                  </template>\r\n                </modal-view>\r\n              </transition>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport CleanserTable from \"../components/TableCleanser.vue\";\r\nimport DataSetTable from \"../components/DataSetTable.vue\";\r\nimport DataSetCompleteTable from \"../components/TableCompleteDataSet.vue\";\r\nimport AddCleanser from \"../components/AddCleanser.vue\";\r\nimport AddCleanedDataSet from \"../components/AddCleanedDataSet.vue\";\r\n\r\nimport Modal from \"../components/Modal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"cleanser-table\": CleanserTable,\r\n    \"data-set-table\": DataSetTable,\r\n    \"add-cleanser\": AddCleanser,\r\n    \"modal-view\": Modal,\r\n    \"data-set-complete-table\": DataSetCompleteTable,\r\n    \"add-cleaned-data-set\": AddCleanedDataSet,\r\n  },\r\n  data() {\r\n    this.autenticateSession();\r\n    return {\r\n      showAddCleanser: false,\r\n      showAddTableData: false,\r\n      showUpdateCleanser: false,\r\n      showApplyCleanser: false,\r\n      componentKey: 0,\r\n      selected_dataset_id: \"\",\r\n      selected_dataset_id_to_apply: \"\",\r\n      selected_storage_type: \"\",\r\n      selected_cleanser_id: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    toggleShowAddCleanser() {\r\n      this.showAddCleanser = !this.showAddCleanser;\r\n    },\r\n    toggleShowApplyCleanser() {\r\n      this.showApplyCleanser = !this.showApplyCleanser;\r\n    },\r\n    toggleShowUpdateCleanser() {\r\n      this.showUpdateCleanser = !this.showUpdateCleanser;\r\n    },\r\n    toggleShowAddTableData() {\r\n      this.showAddTableData = !this.showAddTableData;\r\n    },\r\n    refreshData() {\r\n      this.componentKey += 1;\r\n      this.showAddCleanser = false;\r\n      this.showUpdateCleanser = false;\r\n      this.showAddTableData = false;\r\n      this.showApplyCleanser = false;\r\n    },\r\n    updateCleansers() {\r\n      this.axios\r\n        .post(\r\n          \"http://127.0.0.1:8081/update_cleansers/\" + localStorage.loggedUser\r\n        )\r\n        .then(() => {\r\n          this.refreshData();\r\n        });\r\n    },\r\n    viewDataset(dataset_id, storage_type) {\r\n      console.log(dataset_id);\r\n      this.selected_dataset_id = dataset_id;\r\n      this.selected_storage_type = storage_type;\r\n      this.toggleShowAddTableData();\r\n    },\r\n    autenticateSession() {\r\n      if (localStorage.loggedUser && localStorage.token) {\r\n        console.log(localStorage.token);\r\n        console.log(localStorage.loggedUser);\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/user/validatetoken?token=\" +\r\n              localStorage.token\r\n          )\r\n          .then(() => {\r\n            this.selected_user = localStorage.loggedUser;\r\n            this.axios\r\n              .get(\"http://127.0.0.1:8081/user/\" + this.selected_user)\r\n              .then((res) => {\r\n                this.$store.state.loggedUser = res.data;\r\n                console.log(this.$store.state.loggedUser);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$router.push(\"/\");\r\n          });\r\n      } else {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}