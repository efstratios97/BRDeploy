AAL,CAAc,MAAd,CACE,0BADF,EAEE,KAAK,iCAFP;AAIA,WAAK,KAAL,CACG,IADH,CACQ,uCADR,EACiD,KAAK,QADtD,EAEG,IAFH,CAEQ,UAAC,GAAD,EAAS;AACb,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAhB;AACD,OALH;AAMD;AAxEM;AAnBI,CAAf","sourcesContent":["<template>\r\n  <form>\r\n    <div class=\"mb-3\">\r\n      <label for=\"CleanserName\" class=\"form-label\">\r\n        <b-icon-archive-fill /> Name:</label\r\n      >\r\n      <input\r\n        class=\"input100\"\r\n        id=\"CleanserName\"\r\n        type=\"text\"\r\n        v-model=\"name\"\r\n        placeholder=\"Cleanser Name\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label for=\"description\" class=\"form-label\" id=\"description\">\r\n        <b-icon-pencil-fill /> Description:\r\n      </label>\r\n      <textarea\r\n        class=\"input100\"\r\n        v-model=\"description\"\r\n        placeholder=\"Description\"\r\n        id=\"description\"\r\n      ></textarea>\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label> <b-icon-shield-lock-fill /> Datasets</label>\r\n      <select v-model=\"selected_datasets\" multiple>\r\n        <option\r\n          v-for=\"dataset in datasets\"\r\n          v-bind:value=\"dataset\"\r\n          :key=\"dataset\"\r\n        >\r\n          {{ dataset }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label> <b-icon-shield-lock-fill /> Cleanser Operation Types</label>\r\n      <select v-model=\"selected_cleanser_operation_types\" multiple>\r\n        <option\r\n          v-for=\"operations in cleanser_operation_types\"\r\n          v-bind:value=\"operations\"\r\n          :key=\"operations\"\r\n        >\r\n          {{ operations[0] }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <button class=\"btn-form\" @click=\"createCleanser\">\r\n      <b-icon-upload />\r\n      <b> Add Cleanser</b>\r\n    </button>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      cleaned: \"\",\r\n      description: \"\",\r\n      storageType: \"\",\r\n      file: \"\",\r\n      users: this.getUsersOptions(),\r\n      departments: this.getDepartmentsOptions(),\r\n      cleanser_operation_types: this.getCleanserOperationTypesOptions(),\r\n      datasets: this.getDatasetOptions(),\r\n      selected_users: \"\",\r\n      selected_datasets: \"\",\r\n      selected_departments: \"\",\r\n      selected_cleanser_operation_types: \"\",\r\n      formData: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    getUsersOptions() {\r\n      this.users = this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n    getDepartmentsOptions() {\r\n      this.users = this.axios\r\n        .get(\"http://127.0.0.1:8081/departments\")\r\n        .then((res) => {\r\n          this.departments = res.data;\r\n        });\r\n    },\r\n    createDataSet() {\r\n      this.formData = new FormData();\r\n      this.formData.append(\"name\", this.name);\r\n      this.formData.append(\"file\", this.$refs.file.files[0]);\r\n      this.formData.append(\"cleaned\", this.cleaned);\r\n      this.formData.append(\"description\", this.description);\r\n      this.formData.append(\r\n        \"storage_type\",\r\n        this.storageType === true ? \"cloud\" : \"local\"\r\n      );\r\n      this.formData.append(\"access_user_list\", this.selected_users);\r\n      this.formData.append(\r\n        \"access_business_unit_list\",\r\n        this.selected_departments\r\n      );\r\n      this.axios\r\n        .post(\r\n          \"http://127.0.0.1:8081/create_dataset?uid=\" + localStorage.loggedUser,\r\n          this.formData\r\n        )\r\n        .then((res) => {\r\n          this.$emit(\"close\");\r\n          console.log(res.data);\r\n        });\r\n    },\r\n    getDatasetOptions() {\r\n      this.axios\r\n        .get(\r\n          \"http://127.0.0.1:8081/get_datasets_only_id/\" +\r\n            localStorage.loggedUser\r\n        )\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          this.datasets = res.data.data;\r\n        });\r\n    },\r\n    getCleanserOperationTypesOptions() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/get_cleanser_operation_types\")\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          this.cleanser_operation_types = res.data.data;\r\n        });\r\n    },\r\n    createCleanser() {\r\n      this.formData = new FormData();\r\n      this.formData.append(\"name\", this.name);\r\n      this.formData.append(\"datasets\", this.selected_datasets);\r\n      this.formData.append(\"description\", this.description);\r\n      this.formData.append(\r\n        \"cleanser_operation_types\",\r\n        this.selected_cleanser_operation_types\r\n      );\r\n      this.axios\r\n        .post(\"http://127.0.0.1:8081/create_cleanser\", this.formData)\r\n        .then((res) => {\r\n          this.$emit(\"close\");\r\n          console.log(res.data);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\nlabel {\r\n  display: block;\r\n}\r\n\r\n.btn-form {\r\n  color: white;\r\n  background: #0d6efd;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n\r\nselect {\r\n  color: black;\r\n}\r\n\r\ninput {\r\n  color: black;\r\n}\r\n\r\ntextarea {\r\n  color: black;\r\n}\r\n</style>"],"sourceRoot":""}]}