\n      >\r\n        <input\r\n          class=\"input200\"\r\n          type=\"checkbox\"\r\n          v-model=\"cleaned\"\r\n          id=\"cleaned\"\r\n          placeholder=\"Cleaned\"\r\n        />\r\n        <label for=\"cleaned\"> Cleaned </label><br />\r\n      </div>\r\n\r\n      <div class=\"wrap-input100 validate-input\">\r\n        <textarea\r\n          class=\"input100\"\r\n          v-model=\"description\"\r\n          placeholder=\"Description\"\r\n        ></textarea>\r\n\r\n        <span class=\"focus-input100\"></span>\r\n        <span class=\"symbol-input100\">\r\n          <i class=\"glyphicon glyphicon-list-alt\" aria-hidden=\"true\"></i>\r\n        </span>\r\n      </div>\r\n\r\n      <div\r\n        class=\"wrap-input100 validate-input\"\r\n        data-validate=\"Name is required\"\r\n      >\r\n        <input\r\n          class=\"input200\"\r\n          type=\"checkbox\"\r\n          :storageType=\"storageType\"\r\n          id=\"storage\"\r\n          placeholder=\"Stored in Cloud?\"\r\n        />\r\n        <label for=\"starage\"> Stored in Cloud </label><br />\r\n      </div>\r\n\r\n      <form id=\"form\" enctype=\"multipart/form-data\" @change=\"onUploadFile\">\r\n        <div class=\"mb-3\">\r\n          <label for=\"formFile\" class=\"form-label\">File Input</label>\r\n          <input class=\"form-control\" type=\"file\" id=\"formFile\" :file=\"file\" />\r\n        </div>\r\n      </form>\r\n      <h2>Users to get Access</h2>\r\n      <select v-model=\"selected_users\" multiple>\r\n        <option\r\n          v-for=\"user in users\"\r\n          v-bind:value=\"user.email\"\r\n          :key=\"user.email\"\r\n        >\r\n          {{ user.email }}\r\n        </option>\r\n      </select>\r\n\r\n      <h2>Departments to get Access</h2>\r\n      <select v-model=\"selected_departments\" multiple>\r\n        <option\r\n          v-for=\"department in departments\"\r\n          v-bind:value=\"department.name\"\r\n          :key=\"department.name\"\r\n        >\r\n          {{ department.name }}\r\n        </option>\r\n      </select>\r\n\r\n      <div class=\"container-contact100-form-btn\">\r\n        <button class=\"contact100-form-btn\" @click=\"createDataSet\">\r\n          <b>Add Data Set</b>\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      cleand: \"\",\r\n      description: \"\",\r\n      storageType: \"\",\r\n      file: \"\",\r\n      users: this.getUsersOptions(),\r\n      departments: this.getDepartmentsOptions(),\r\n      selected_users: \"\",\r\n      selected_departments: \"\",\r\n      dataset: {},\r\n    };\r\n  },\r\n  methods: {\r\n    getUsersOptions() {\r\n      this.users = this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n    getDepartmentsOptions() {\r\n      this.users = this.axios\r\n        .get(\"http://127.0.0.1:8081/departments\")\r\n        .then((res) => {\r\n          this.departments = res.data;\r\n        });\r\n    },\r\n    createDataSet() {\r\n      this.dataset = {\r\n        name: this.name,\r\n        cleaned: this.cleaned,\r\n        data: this.file,\r\n        access_user_list: this.selected_users[0],\r\n        access_business_unit_list: this.selected_departments[0],\r\n        description: this.description,\r\n        storage_type: this.storageType,\r\n      };\r\n      const jsonString = JSON.stringify(this.dataset);\r\n      console.log(jsonString);\r\n      this.axios\r\n        .post(\r\n          \"http://127.0.0.1:8081/create_dataset?uid=\" + localStorage.loggedUser,\r\n          jsonString\r\n        )\r\n        .then((res) => {\r\n          console.log(res.data);\r\n        });\r\n    },\r\n\r\n    onUploadFile(event) {\r\n      const file = event.target.files[0];\r\n      console.log(file);\r\n      this.axios\r\n        .post(\"http://127.0.0.1:8081/upload_dataset\", file, {\r\n          headers: {\r\n            \"Content-Type\": \"multipart/form-data\",\r\n          },\r\n        })\r\n        .then(console.log)\r\n        .catch(console.error);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/*//////////////////////////////////////////////////////////////////\r\n[ FONT ]*/\r\n\r\n/*//////////////////////////////////////////////////////////////////\r\n[ RESTYLE TAG ]*/\r\n\r\n* {\r\n  margin: 0px;\r\n  padding: 0px;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody,\r\nhtml {\r\n  height: 100%;\r\n  font-family: sans-serif;\r\n}\r\n\r\n/*---------------------------------------------*/\r\na {\r\n  font-size: 14px;\r\n  line-height: 1.7;\r\n  color: #666666;\r\n  margin: 0px;\r\n  transition: all 0.4s;\r\n  -webkit-transition: all 0.4s;\r\n  -o-transition: all 0.4s;\r\n  -moz-transition: all 0.4s;\r\n}\r\nhr {\r\n  background: dodgerblue;\r\n  height: 20px;\r\n  border: none;\r\n}\r\n\r\nlabel {\r\n  color: white;\r\n  margin: 5px 0px;\r\n  width: 200px;\r\n  font-size: 20px;\r\n}\r\n.input200 {\r\n  float: center;\r\n  margin: 5px 5px;\r\n  transform: scale(1.8);\r\n}\r\na:focus {\r\n  outline: none !important;\r\n}\r\n\r\na:hover {\r\n  text-decoration: none;\r\n  color: #57b846;\r\n}\r\n\r\n/*---------------------------------------------*/\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6 {\r\n  margin: 0px;\r\n  color: white;\r\n}\r\n\r\np {\r\n  font-size: 14px;\r\n  line-height: 1.7;\r\n  color: #666666;\r\n  margin: 0px;\r\n}\r\n\r\nul,\r\nli {\r\n  margin: 0px;\r\n  list-style-type: none;\r\n}\r\n\r\n/*---------------------------------------------*/\r\ninput {\r\n  outline: none;\r\n  border: none;\r\n}\r\n\r\ntextarea {\r\n  outline: none;\r\n  border: none;\r\n}\r\n\r\ntextarea:focus,\r\ninput:focus {\r\n  border-color: transparent !important;\r\n}\r\n\r\ninput:focus::-webkit-input-placeholder {\r\n  color: transparent;\r\n}\r\ninput:focus:-moz-placeholder {\r\n  color: transparent;\r\n}\r\ninput:f