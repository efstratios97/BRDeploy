AAC,WAAL,CAAiB,OAAjB,GAA2B,GAAG,CAAC,IAAJ,CAAS,WAApC;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,GAAqB,GAAG,CAAC,IAAJ,CAAS,KAA9B;AACA,QAAA,MAAI,CAAC,OAAL,CAAa,OAAb,GAAuB,GAAG,CAAC,IAAJ,CAAS,OAAhC;AACD,OAXH;AAYD,KAxDM;AAyDP,IAAA,WAzDO,yBAyDO;AAAA;;AACZ,UAAI,UAAS,GAAI;AACf,QAAA,gBAAgB,EAAE,KAAK,cAAL,CAAoB,KADvB;AAEf,QAAA,YAAY,EAAE,KAAK,qBAAL,CAA2B,KAF1B;AAGf,QAAA,aAAa,EAAE,KAAK,aAAL,CAAmB,KAHnB;AAIf,QAAA,gBAAgB,EAAE,KAAK,WAAL,CAAiB,KAJpB;AAKf,QAAA,OAAO,EAAE,KAAK,OAAL,CAAa,KALP;AAMf,QAAA,GAAG,EAAE,KAAK,KAAL,CAAW;AAND,OAAjB;AAQA,UAAM,UAAS,GAAI,IAAI,CAAC,SAAL,CAAe,UAAf,CAAnB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,UAAZ;AACA,WAAK,KAAL,CACG,IADH,CAEI,uCACE,KAAK,gBADP,GAEE,oBAJN,EAKI,UALJ,EAOG,IAPH,CAOQ,UAAC,QAAD,EAAc;AAClB,QAAA,MAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAAT,CAAc,GAA1B,CADkB,CAElB;;AACA,YAAI,cAAa,GAAI,QAAQ,CAAC,aAAT,CAAuB,QAAvB,CAArB;AACA,QAAA,cAAc,CAAC,YAAf,CAA4B,MAA5B,EAAoC,iBAApC;AACA,YAAI,CAAA,GAAI,QAAQ,CAAC,cAAT,CAAwB,QAAQ,CAAC,IAAT,CAAc,MAAtC,CAAR;AACA,QAAA,cAAc,CAAC,WAAf,CAA2B,CAA3B;AACA,QAAA,QAAQ,CAAC,IAAT,CAAc,WAAd,CAA0B,cAA1B;AACD,OAfH;AAgBD;AApFM;AAnCI,CAAf","sourcesContent":["<template>\r\n  <div>\r\n    <h1 class=\"h1_widget\">Statistical Analysis</h1>\r\n    <form>\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-9\">\r\n            <div class=\"row justify-content-center\">\r\n              <div class=\"col-4\">\r\n                <h3 class=\"h3_widget\">Business Unit:</h3>\r\n                <Multiselect\r\n                  style=\"color: white\"\r\n                  v-model=\"business_units.value\"\r\n                  v-bind=\"business_units\"\r\n                  :searchable=\"true\"\r\n                  mode=\"multiple\"\r\n                />\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <h3 class=\"h3_widget\">Business Cap.:</h3>\r\n                <Multiselect\r\n                  style=\"color: white\"\r\n                  v-model=\"business_capabilities.value\"\r\n                  v-bind=\"business_capabilities\"\r\n                  :searchable=\"true\"\r\n                  mode=\"multiple\"\r\n                />\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <h3 class=\"h3_widget\">Y Axis:</h3>\r\n                <Multiselect\r\n                  style=\"color: white\"\r\n                  v-model=\"bubble_y_axis.value\"\r\n                  v-bind=\"bubble_y_axis\"\r\n                  :searchable=\"true\"\r\n                />\r\n              </div>\r\n              <br />\r\n              <div class=\"row justify-content-center pb-5 pt-2\">\r\n                <div class=\"col-4\">\r\n                  <h3 class=\"h3_widget\">Top:</h3>\r\n                  <Multiselect\r\n                    style=\"color: white\"\r\n                    v-model=\"range.value\"\r\n                    v-bind=\"range\"\r\n                    :searchable=\"true\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-4\">\r\n                  <h3 class=\"h3_widget\">Sorting:</h3>\r\n                  <Multiselect\r\n                    style=\"color: white\"\r\n                    v-model=\"sorting.value\"\r\n                    v-bind=\"sorting\"\r\n                    :searchable=\"true\"\r\n                  />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-3 d-flex align-items-center justify-content-center\">\r\n            <button @click=\"send_inputs\">\r\n              <div class=\"container\">\r\n                <div class=\"row justify-content-md-center\">\r\n                  <div class=\"col-1\">\r\n                    <b-icon-funnel-fill />\r\n                  </div>\r\n                  <div class=\"col-4\">\r\n                    <b>Apply</b>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <div class=\"container\">\r\n      <div class=\"row\">\r\n        <div class=\"col d-flex justify-content-center\">\r\n          <div v-html=\"html\" style=\"resize: both; overflow: auto\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"@vueform/multiselect\";\r\n\r\nexport default {\r\n  components: {\r\n    Multiselect,\r\n  },\r\n  props: [\"selected_dataset\"],\r\n  data() {\r\n    return {\r\n      business_units: {\r\n        value: [\"All\"],\r\n        options: [],\r\n      },\r\n      business_capabilities: {\r\n        value: [\"All\"],\r\n        options: [],\r\n      },\r\n      bubble_y_axis: {\r\n        value: \"Select\",\r\n        options: [],\r\n      },\r\n      bubble_size: {\r\n        value: \"Select\",\r\n        options: [],\r\n      },\r\n      range: {\r\n        value: \"All\",\r\n        options: [],\r\n      },\r\n      sorting: {\r\n        value: \"Descending\",\r\n        options: [],\r\n      },\r\n      html: this.show_graph(),\r\n      params: this.get_params(),\r\n    };\r\n  },\r\n  methods: {\r\n    show_graph() {\r\n      const bokehVersion = \"2.3.1\";\r\n      let link = document.createElement(\"link\");\r\n      link.setAttribute(\"rel\", \"stylesheet\");\r\n      link.setAttribute(\r\n        \"href\",\r\n        \"https://cdn.bokeh.org/bokeh/release/bokeh-0.13.0\" + \".min.css\"\r\n      );\r\n      link.setAttribute(\"type\", \"text/css\");\r\n      document.head.appendChild(link);\r\n\r\n      let script = document.createElement(\"script\");\r\n      // Insert js code in the header\r\n      script.setAttribute(\r\n        \"src\",\r\n        \"https://cdn.bokeh.org/bokeh/release/bokeh-\" + bokehVersion + \".min.js\"\r\n      );\r\n      script.async = \"async\";\r\n      document.head.appendChild(script);\r\n\r\n      // cdn js is loaded after requesting bokeh parameters\r\n      script.onload = () => {\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/show_plotter/\" +\r\n              this.selected_dataset +\r\n              \"/simple_statistics\"\r\n          )\r\n          .then((response) => {\r\n            console.log(response);\r\n            this.html = response.data.div;\r\n            // insert drawing script code\r\n            let bokehRunScript = document.createElement(\"SCRIPT\");\r\n            bokehRunScript.setAttribute(\"type\", \"text/javascript\");\r\n            let t = document.createTextNode(response.data.script);\r\n            bokehRunScript.appendChild(t);\r\n            document.body.appendChild(bokehRunScript);\r\n            // Close the wait screen after the drawing code is executed\r\n            // _this.loading = false;\r\n          });\r\n      };\r\n    },\r\n    get_params() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/get_params/\" + this.selected_dataset)\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          this.params = res.data;\r\n          this.business_units.options = res.data.list_bu;\r\n          this.business_capabilities.options = res.data.list_bc;\r\n          this.bubble_y_axis.options = res.data.bubble_y_axis;\r\n          this.bubble_size.options = res.data.bubble_size;\r\n          this.range.options = res.data.range;\r\n          this.sorting.options = res.data.sorting;\r\n        });\r\n    },\r\n    send_inputs() {\r\n      var user_input = {\r\n        responsible_unit: this.business_units.value,\r\n        supported_bc: this.business_capabilities.value,\r\n        bubble_y_axis: this.bubble_y_axis.value,\r\n        bubble_size_axis: this.bubble_size.value,\r\n        sorting: this.sorting.value,\r\n        top: this.range.value,\r\n      };\r\n      const jsonString = JSON.stringify(user_input);\r\n      console.log(jsonString);\r\n      this.axios\r\n        .post(\r\n          \"http://127.0.0.1:8081/post_inputs/\" +\r\n            this.selected_dataset +\r\n            \"/simple_statistics\",\r\n          jsonString\r\n        )\r\n        .then((response) => {\r\n          this.html = response.data.div;\r\n          // insert drawing script code\r\n          let bokehRunScript = document.createElement(\"SCRIPT\");\r\n          bokehRunScript.setAttribute(\"type\", \"text/javascript\");\r\n          let t = document.createTextNode(response.data.script);\r\n          bokehRunScript.appendChild(t);\r\n          document.body.appendChild(bokehRunScript);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style src=\"@vueform/multiselect/themes/default.css\">\r\n</style>\r\n<style scoped>\r\n.h1_widget {\r\n  text-align: center;\r\n  color: white;\r\n  padding-bottom: 10px;\r\n}\r\n.h3_widget {\r\n  text-align: center;\r\n  color: white;\r\n}\r\n\r\nbutton {\r\n  width: 250px;\r\n  height: 35px;\r\n  background: #fff;\r\n  border: 1px solid #fff;\r\n  cursor: pointer;\r\n  border-radius: 2px;\r\n  color: #050504;\r\n  font-family: \"Exo\", sans-serif;\r\n  font-size: 16px;\r\n  font-weight: 400;\r\n  padding: 6px;\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton:hover {\r\n  opacity: 0.8;\r\n}\r\n</style>"],"sourceRoot":""}]}