</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      departments: this.listDepartments(),\r\n    };\r\n  },\r\n  methods: {\r\n    deleteDep(dep_id) {\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_department/\" +\r\n            dep_id +\r\n            \"/\" +\r\n            localStorage.loggedUser\r\n        )\r\n        .then(() => {\r\n          this.listDepartments();\r\n        });\r\n    },\r\n    listDepartments() {\r\n      this.axios.get(\"http://127.0.0.1:8081/departments\").then((res) => {\r\n        console.log(res.data);\r\n        this.departments = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: dodgerblue;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}