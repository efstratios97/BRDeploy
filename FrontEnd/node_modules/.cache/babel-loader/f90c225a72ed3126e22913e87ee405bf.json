=\"width: 3em\"></Column>\r\n    <Column field=\"user_id\" header=\"ID\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"first_name\"\r\n      header=\"First Name\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column\r\n      field=\"last_name\"\r\n      header=\"Last Name\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column field=\"email\" header=\"Email\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"business_unit\"\r\n      header=\"Department\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column\r\n      field=\"role_manager\"\r\n      header=\"Role Manager\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column field=\"admin\" header=\"Admin\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column header=\"Ad hoc Operations\" style=\"min-width: auto\">\r\n      <template #body>\r\n        <div class=\"btn-align-td\">\r\n          <button\r\n            v-on:click=\"deleteUser(this.selected_user.user_id)\"\r\n            class=\"btn btn-secondary\"\r\n            v-tooltip=\"'Select User first'\"\r\n          >\r\n            <b-icon-trash-fill />\r\n          </button>\r\n        </div>\r\n        <ProgressBar mode=\"indeterminate\" v-if=\"user_operations\" /> </template\r\n    ></Column>\r\n  </DataTable>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: this.listUsers(),\r\n      selected_user: \"\",\r\n      user_operations: false,\r\n    };\r\n  },\r\n  methods: {\r\n    deleteUser(user_id) {\r\n      if (user_id === undefined) {\r\n        this.$toast.add({\r\n          severity: \"warn\",\r\n          summary: \"No User selected\",\r\n          detail: \"Please select User first\",\r\n          life: 3000,\r\n        });\r\n      } else {\r\n        this.user_operations = true;\r\n      }\r\n      const issued_user = localStorage.loggedUser;\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_user/\" + user_id + \"/\" + issued_user\r\n        )\r\n        .then(() => {\r\n          this.user_operations = false;\r\n          this.listUsers();\r\n          this.$toast.add({\r\n            severity: \"success\",\r\n            summary: \"User Deletion Successful\",\r\n            detail: \"The selected User: \" + user_id + \" was deleted\",\r\n            life: 3000,\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.user_operations = false;\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"User Deletion Unsuccessful\",\r\n            detail:\r\n              \"The selected User: \" +\r\n              user_id +\r\n              \" could not be deleted!\\n Check if you have admin rights\",\r\n            life: 3000,\r\n          });\r\n        });\r\n    },\r\n    listUsers() {\r\n      this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: #0c8cd3;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}