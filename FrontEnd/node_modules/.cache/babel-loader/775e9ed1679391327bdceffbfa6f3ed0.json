rows=\"10\"\r\n          dataKey=\"id\"\r\n          :rowHover=\"true\"\r\n          filterDisplay=\"menu\"\r\n          :loading=\"loading\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 25, 50]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\r\n          :globalFilterFields=\"[\r\n            'name',\r\n            'country.name',\r\n            'representative.name',\r\n            'status',\r\n          ]\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div class=\"p-d-flex p-jc-between p-ai-center\">\r\n              <h5 class=\"p-m-0\">Customers</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText placeholder=\"Keyword Search\" />\r\n              </span>\r\n            </div>\r\n          </template>\r\n          <template #empty> No customers found. </template>\r\n          <template #loading> Loading customers data. Please wait. </template>\r\n          <Column selectionMode=\"multiple\" style=\"min-width: 3rem\"></Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n    <DataTableDoc />\r\n  </div>\r\n  <!-- <div class=\"table-responsive\">\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\">ID</th>\r\n          <th>Name</th>\r\n          <th scope=\"col\">Description</th>\r\n          <th scope=\"col\">Cleaned</th>\r\n          <th scope=\"col\">Owner</th>\r\n          <th scope=\"col\">Storage Location</th>\r\n          <th scope=\"col\">Access Users</th>\r\n          <th scope=\"col\">Access BUs</th>\r\n          <th scope=\"col\">Hash of Dataset</th>\r\n          <th scope=\"col\">Data Set Operations</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"dataset in datasets\" :key=\"dataset.dataset_id\">\r\n          <td>{{ dataset.dataset_id }}</td>\r\n          <td>{{ dataset.name }}</td>\r\n          <td>{{ dataset.description }}</td>\r\n          <td>{{ dataset.cleaned === 0 ? \"No\" : \"Yes\" }}</td>\r\n          <td>{{ enumOwner(dataset.owner) }}</td>\r\n          <td>{{ dataset.storage_type }}</td>\r\n          <td>\r\n            <div class=\"dropdown\">\r\n              <button\r\n                class=\"btn btn-secondary dropdown-toggle\"\r\n                type=\"button\"\r\n                id=\"dropdownMenuButton1\"\r\n                data-bs-toggle=\"dropdown\"\r\n                aria-expanded=\"false\"\r\n              >\r\n                See Users\r\n              </button>\r\n              <ul class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton1\">\r\n                <li v-for=\"user in dataset.access_user_list\" :key=\"user\">\r\n                  <p class=\"dropdown-item\">{{ enumUser(user) }}</p>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div class=\"dropdown\">\r\n              <button\r\n                class=\"btn btn-secondary dropdown-toggle\"\r\n                type=\"button\"\r\n                id=\"dropdownMenuButton1\"\r\n                data-bs-toggle=\"dropdown\"\r\n                aria-expanded=\"false\"\r\n              >\r\n                See Departments\r\n              </button>\r\n              <ul class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton1\">\r\n                <li v-for=\"dep in dataset.access_business_unit_list\" :key=\"dep\">\r\n                  <p class=\"dropdown-item\">{{ dep }}</p>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </td>\r\n          <td>{{ dataset.hash_of_dataset }}</td>\r\n          <td>\r\n            <div class=\"btn-align-td\">\r\n              <button\r\n                v-on:click=\"\r\n                  deleteDataSet(dataset.dataset_id, dataset.storage_type)\r\n                \"\r\n                class=\"btn btn-secondary\"\r\n              >\r\n                <i class=\"fa fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n            <div class=\"btn-align-td\">\r\n              <button\r\n                v-on:click=\"\r\n                  emitDataset(dataset.dataset_id, dataset.storage_type)\r\n                \"\r\n                class=\"btn btn-secondary\"\r\n              >\r\n                <b-icon-eye-fill></b-icon-eye-fill>\r\n              </button>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div> -->\r\n</template>\r\n \r\n<script>\r\nimport DataTableDoc from \"./DataTableDoc\";\r\nimport DataTable from \"primevue/datatable\";\r\nimport Column from \"primevue/column\";\r\n\r\nexport default {\r\n  components: {\r\n    DataTableDoc,\r\n    Column,\r\n    DataTable,\r\n  },\r\n  data() {\r\n    return {\r\n      users: this.listUsers(),\r\n      datasets: this.listDatasets(),\r\n      columns: [\r\n        { prop: \"name\", name: \"First\" },\r\n        { prop: \"details\", name: \"Second\" },\r\n      ],\r\n      rows: [{ name: \"1\", details: \"Item 1\" }],\r\n      grid: 1,\r\n    };\r\n  },\r\n  methods: {\r\n    enumOwner(owner) {\r\n      for (let i = 0; i < this.users.length; i++) {\r\n        if (this.users[i][\"user_id\"] === owner) {\r\n          return this.users[i][\"email\"];\r\n        }\r\n      }\r\n      return owner;\r\n    },\r\n    enumUser(user) {\r\n      for (let i = 0; i < this.users.length; i++) {\r\n        if (this.users[i][\"user_id\"] === user) {\r\n          return this.users[i][\"email\"];\r\n        }\r\n      }\r\n      return user;\r\n    },\r\n    deleteDataSet(dataset_id, storageType) {\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_dataset/\" +\r\n            dataset_id +\r\n            \"/\" +\r\n            storageType\r\n        )\r\n        .then(() => {\r\n          console.log(\"Refreshing\");\r\n          this.listDatasets();\r\n        });\r\n    },\r\n    emitDataset(dataset_id, storage_type) {\r\n      this.$emit(\"view\", dataset_id, storage_type);\r\n    },\r\n    listDatasets() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/get_datasets/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          this.datasets = res.data.data;\r\n        });\r\n    },\r\n    listUsers() {\r\n      this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        console.log(res.data);\r\n        this.users = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep(.p-paginator) {\r\n  .p-paginator-current {\r\n    margin-left: auto;\r\n  }\r\n}\r\n::v-deep(.p-progressbar) {\r\n  height: 0.5rem;\r\n  background-color: #d8dadc;\r\n  .p-progressbar-value {\r\n    background-color: #607d8b;\r\n  }\r\n}\r\n::v-deep(.p-datepicker) {\r\n  min-width: 25rem;\r\n  td {\r\n    font-weight: 400;\r\n  }\r\n}\r\n::v-deep(.p-datatable.p-datatable-customers) {\r\n  .p-datatable-header {\r\n    padding: 1rem;\r\n    text-align: left;\r\n    font-size: 1.5rem;\r\n  }\r\n  .p-paginator {\r\n    padding: 1rem;\r\n  }\r\n  .p-datatable-thead > tr > th {\r\n    text-align: left;\r\n  }\r\n  .p-datatable-tbody > tr > td {\r\n    cursor: auto;\r\n  }\r\n  .p-dropdown-label:not(.p-placeholder) {\r\n    text-transform: uppercase;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}