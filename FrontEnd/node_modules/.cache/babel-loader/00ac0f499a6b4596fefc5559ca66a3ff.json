,IAAA,WAAW,EAAE;AAAA,eAAA,MAAA;AAAA,eAAA;AAAA;AAmCP,GArCT,E;qBAIE;AAAA,aAgCO,CAhCP,YAAA,CAgCO,eAhCP,EAgCO,IAhCP,EAgCO;AA/BM,QAAA,MAAM,EAAA,OAAA,CACf;AAAA,iBAeM,CAfN,WAeM,CAfN;AAAA,SADe,CA+BZ;AAbM,QAAA,KAAK,EAAA,OAAA,CAAE;AAAA,iBAAqC,CAArC,WAAqC,CAArC;AAAA,SAAF,CAaX;AAZM,QAAA,OAAO,EAAA,OAAA,CAChB;AAAA,iBAMK,CANL,WAMK,CANL;AAAA,SADgB,CAYb;AAHM,QAAA,MAAM,EAAA,OAAA,CACf;AAAA,iBAAkE,CAAlE,YAAA,CAAkE,iBAAlE,EAAkE;AAA1D,YAAA,IAAI,EAAC,aAAqD;AAAvC,YAAA,KAAK,EAAC,MAAiC;AAAzB,YAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,qBAAE,QAAA,CAAA,YAAA,EAAF;AAAA,aAAA;AAAoB,WAAlE,CAAkE,CAAlE;AAAA,SADe,CAGZ;;AAAA,OAhCP,CAgCO,CAhCP;AAAA,K;;GAJF,E,CAAA,E,WAAA,CAiDI,EAVJ,WAUI,CA7IN,CA8II,CAjJN,C","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"body\"></div>\r\n    <div class=\"grad\"></div>\r\n    <div class=\"header\">\r\n      <div class=\"container\" id=\"container\">\r\n        <div class=\"form-container sign-in-container\">\r\n          <form action=\"#\">\r\n            <div class=\"p-grid\">\r\n              <div class=\"p-col inline-block\">\r\n                <img\r\n                  src=\"~@/assets/BR_LOGO.png\"\r\n                  alt=\"My image\"\r\n                  style=\"width: 80px; height: 70px; margin-top: -50px\"\r\n                />\r\n              </div>\r\n              <div class=\"p-col inline-block\">\r\n                <img\r\n                  src=\"~@/assets/LTEP_LOGO_2.png\"\r\n                  alt=\"My image\"\r\n                  style=\"width: 110px; height: 90px; margin-top: -50px\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <h4 style=\"color: black; margin-bottom: -5px; margin-top: -5px\">\r\n              <b>BR Architecture Manager</b>\r\n            </h4>\r\n            <h6 style=\"margin-bottom: 15px\">by <b>LTEP Technologies</b></h6>\r\n            <hr style=\"margin: 1px; color: black\" />\r\n            <h1 style=\"color: black\">Sign in</h1>\r\n            <span>Enter your Credentials</span>\r\n            <input v-model=\"email\" type=\"email\" placeholder=\"Email\" />\r\n            <input v-model=\"password\" type=\"password\" placeholder=\"Password\" />\r\n            <a href=\"javascript:void(0);\" @click=\"toggleForgotPassword()\"\r\n              >Forgot your password?</a\r\n            >\r\n            <button @click=\"login()\">Sign In</button>\r\n          </form>\r\n        </div>\r\n        <div class=\"overlay-container\">\r\n          <div class=\"overlay\">\r\n            <div class=\"overlay-panel overlay-left\">\r\n              <h1>Welcome Back!</h1>\r\n              <p>\r\n                To keep connected with us please login with your personal info\r\n              </p>\r\n              <button class=\"ghost\" id=\"signIn\">Sign In</button>\r\n            </div>\r\n            <div class=\"overlay-panel overlay-right\">\r\n              <h1>Welcome!</h1>\r\n              <p>\r\n                Thank you for choosing to use the\r\n                <b>BR Architecture Manager</b> powered by\r\n                <b>LTEP Technologies</b>\r\n              </p>\r\n              <button class=\"ghost\" id=\"signUp\" @click=\"toggleSignUp()\">\r\n                Sign Up\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <Dialog\r\n        v-model:visible=\"forgot_password\"\r\n        :breakpoints=\"{ '960px': '75vw', '640px': '100vw' }\"\r\n      >\r\n        <Card>\r\n          <template #header>\r\n            <img\r\n              alt=\"User Forgot PWD\"\r\n              src=\"~@/assets/forgot_pwd.png\"\r\n              style=\"width: 400px; height: auto\"\r\n            />\r\n          </template>\r\n          <template #title>\r\n            <h1 style=\"color: black\">Password Recovery</h1></template\r\n          >\r\n          <template #content>\r\n            <h3>\r\n              Due to the sensitivity of the data contained in the BR\r\n              Architecture Manager please contact your responsible BR Enterprise\r\n              Architecture Manager Admin: <br /><br />\r\n              Johannes Lederle : johannes.lederle@br.de\r\n            </h3>\r\n          </template>\r\n          <template #footer>\r\n            <Button\r\n              icon=\"pi pi-check\"\r\n              label=\"Okay\"\r\n              @click=\"toggleForgotPassword()\"\r\n            />\r\n          </template>\r\n        </Card>\r\n      </Dialog>\r\n\r\n      <Dialog\r\n        v-model:visible=\"sign_up\"\r\n        :breakpoints=\"{ '960px': '75vw', '640px': '100vw' }\"\r\n      >\r\n        <Card>\r\n          <template #header>\r\n            <div class=\"p-grid\">\r\n              <div class=\"p-col inline-block\">\r\n                <img\r\n                  src=\"~@/assets/BR_LOGO.png\"\r\n                  alt=\"My image\"\r\n                  style=\"width: 80px; height: 70px\"\r\n                />\r\n              </div>\r\n              <div class=\"p-col inline-block\">\r\n                <img\r\n                  src=\"~@/assets/LTEP_LOGO_2.png\"\r\n                  alt=\"My image\"\r\n                  style=\"width: 110px; height: 90px\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </template>\r\n          <template #title> <h1 style=\"color: black\">SIGN UP</h1></template>\r\n          <template #content>\r\n            <h3>\r\n              Thank you for choosing to use the BR Architecture Manager! <br />\r\n              Please contact your responsible BR Enterprise Architecture Manager\r\n              Admin: <br />\r\n              <br />\r\n              Johannes Lederle : johannes.lederle@br.de\r\n            </h3>\r\n          </template>\r\n          <template #footer>\r\n            <Button icon=\"pi pi-check\" label=\"Okay\" @click=\"toggleSignUp()\" />\r\n          </template>\r\n        </Card>\r\n      </Dialog>\r\n\r\n      <footer>\r\n        <p>\r\n          Created by LTEP Technologies. Copyright 2021 Â©\r\n          <a\r\n            target=\"_blank\"\r\n            href=\"https://de.linkedin.com/in/leo-thieroff-a5b965161\"\r\n            >More</a\r\n          >.\r\n        </p>\r\n      </footer>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      users: \"\",\r\n      forgot_password: false,\r\n      sign_up: false,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleForgotPassword() {\r\n      this.forgot_password = !this.forgot_password;\r\n    },\r\n    toggleSignUp() {\r\n      this.sign_up = !this.sign_up;\r\n    },\r\n    findUser(value) {\r\n      if (value.email == this.email) {\r\n        return value;\r\n      }\r\n    },\r\n    login() {\r\n      this.$axios\r\n        .get(\"/user/auth?email=\" + this.email + \"&passwd=\" + this.password)\r\n        .then((res) => {\r\n          this.users = res.data;\r\n          this.allUsers = res.data;\r\n          localStorage.token = res.data.token;\r\n          this.$axios.get(\"/users\").then((res) => {\r\n            this.users = res.data;\r\n            localStorage.loggedUser = this.users.filter(\r\n              this.findUser\r\n            )[0].user_id;\r\n            for (let i = 0; i < this.users.length; i++) {\r\n              if (this.users[i][\"user_id\"] === localStorage.loggedUser) {\r\n                this.$toast.add({\r\n                  severity: \"success\",\r\n                  summary: \"Welcome \" + this.users[i][\"first_name\"],\r\n                  detail: \"You successfully logged in\",\r\n                  life: 3000,\r\n                });\r\n                this.users = null;\r\n                this.$router.push(\"/mainmenu\");\r\n              }\r\n            }\r\n          });\r\n          this.$router.push(\"/\");\r\n        })\r\n        .catch(() => {\r\n          this.$toast.add({\r\n            severity: \"error\",\r