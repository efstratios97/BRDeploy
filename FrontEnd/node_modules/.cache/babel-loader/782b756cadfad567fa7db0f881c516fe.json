M;AACV,QAAA,KAAI,CAAC,SAAL;AACD,OANH;AAOD,KAVM;AAWP,IAAA,SAXO,uBAWK;AAAA;;AACV,WAAK,KAAL,CAAW,GAAX,CAAe,6BAAf,EAA8C,IAA9C,CAAmD,UAAC,GAAD,EAAS;AAC1D,QAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAhB;AACA,QAAA,MAAI,CAAC,KAAL,GAAa,GAAG,CAAC,IAAjB;AACD,OAHD;AAID;AAhBM;AAPI,CAAf","sourcesContent":["<template>\r\n  <DataTable\r\n    :value=\"users\"\r\n    v-model:selection=\"selected_user\"\r\n    dataKey=\"user_id\"\r\n    :paginator=\"true\"\r\n    :rows=\"10\"\r\n    filterDisplay=\"menu\"\r\n    paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\r\n    :rowsPerPageOptions=\"[10, 25, 50]\"\r\n    currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\r\n    responsiveLayout=\"scroll\"\r\n  >\r\n    <template #header>\r\n      <div class=\"p-d-flex p-jc-between p-ai-center\">\r\n        <h5 class=\"p-m-0\">Users</h5>\r\n        <span class=\"p-input-icon-left\">\r\n          <i class=\"pi pi-search\" />\r\n          <InputText placeholder=\"Keyword Search\" />\r\n        </span>\r\n      </div>\r\n    </template>\r\n    <template #empty> Loading Users. Please wait. </template>\r\n    <template #loading> Loading Users. Please wait. </template>\r\n    <Column selectionMode=\"single\" headerStyle=\"width: 3em\"></Column>\r\n    <Column field=\"user_id\" header=\"ID\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"first_name\"\r\n      header=\"First Name\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column\r\n      field=\"last_name\"\r\n      header=\"Last Name\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column field=\"email\" header=\"Email\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column\r\n      field=\"business_unit\"\r\n      header=\"Department\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column\r\n      field=\"role_manager\"\r\n      header=\"Role Manager\"\r\n      sortable\r\n      style=\"min-width: auto\"\r\n    >\r\n    </Column>\r\n    <Column field=\"admin\" header=\"Admin\" sortable style=\"min-width: auto\">\r\n    </Column>\r\n    <Column header=\"Ad hoc Operations\" style=\"min-width: auto\">\r\n      <template #body>\r\n        <div class=\"btn-align-td\">\r\n          <button\r\n            v-on:click=\"deleteUser(this.selected_user.user_id)\"\r\n            class=\"btn btn-secondary\"\r\n          >\r\n            <b-icon-trash-fill />\r\n          </button>\r\n        </div> </template\r\n    ></Column>\r\n  </DataTable>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: this.listUsers(),\r\n      selected_user: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    deleteUser(user_id) {\r\n      const issued_user = localStorage.loggedUser;\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_user/\" + user_id + \"/\" + issued_user\r\n        )\r\n        .then(() => {\r\n          this.listUsers();\r\n        });\r\n    },\r\n    listUsers() {\r\n      this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        console.log(res.data);\r\n        this.users = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: #0c8cd3;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}