  <i class=\"fa fa-bars\"></i>\r\n      </button>\r\n      <ul v-if=\"detailsvisible\">\r\n        <li>ID: {{ datasetId }}</li>\r\n        <li>Description: {{ description }}</li>\r\n        <li>Cleaned: {{ cleaned === 0 ? \"Yes\" : \"No\" }}</li>\r\n        <li>Owner: {{ owner }}</li>\r\n        <li>Storage Location: {{ storageType }}</li>\r\n        <li>Access Users: {{ accessUserList }}</li>\r\n        <li>Access BUs: {{ accessBusinessUnitList }}</li>\r\n        <li>Hash of Dataset: {{ hashOfDataset }}</li>\r\n      </ul>\r\n      <button v-on:click=\"deleteDataSet\" class=\"btn\">\r\n        <i class=\"fa fa-bars\"></i>\r\n      </button>\r\n    </li>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  props: [\r\n    \"datasetId\",\r\n    \"name\",\r\n    \"accessUserList\",\r\n    \"accessBusinessUnitList\",\r\n    \"description\",\r\n    \"owner\",\r\n    \"hashOfDataset\",\r\n    \"storageType\",\r\n    \"cleaned\",\r\n  ],\r\n  data() {\r\n    return {\r\n      detailsvisible: false,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleDetails() {\r\n      this.detailsvisible = !this.detailsvisible;\r\n    },\r\n\r\n    deleteDataSet() {\r\n      console.log(this.datasetId);\r\n      this.axios\r\n        .delete(\r\n          \"http://127.0.0.1:8081/delete_dataset/\" +\r\n            this.datasetId +\r\n            \"/\" +\r\n            this.storageType\r\n        )\r\n        .then(() => {\r\n          this.flashMessage.show({\r\n            title: \"Sucesso\",\r\n            status: \"success\",\r\n            message: \"Reload to see changes\",\r\n          });\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"https://fonts.googleapis.com/css2?family=Jost&display=swap\");\r\n@import url(\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\");\r\n\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n\r\nhtml {\r\n  font-family: \"Jost\", sans-serif;\r\n}\r\n\r\nbody {\r\n  margin: 0;\r\n}\r\n\r\nheader {\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\r\n  margin: 3rem auto;\r\n  border-radius: 10px;\r\n  padding: 1rem;\r\n  background-color: #58004d;\r\n  color: white;\r\n  text-align: center;\r\n  width: 90%;\r\n  max-width: 50rem;\r\n}\r\n.nextto {\r\n  display: inline-block;\r\n  width: 25%;\r\n  margin: 10px;\r\n}\r\n\r\n#app ul {\r\n  margin: 0;\r\n  padding: 0;\r\n  list-style: none;\r\n  list-style-type: none;\r\n}\r\n\r\n#app li,\r\n#app form {\r\n  color: white;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\r\n  margin: 2rem auto;\r\n  border-radius: 10px;\r\n  padding: 1rem;\r\n  text-align: center;\r\n  width: 100%;\r\n  max-width: 50rem;\r\n  list-style-type: none;\r\n  border-style: solid;\r\n  border-color: DodgerBlue;\r\n  overflow-wrap: break-word;\r\n}\r\n\r\n#app h2 {\r\n  color: white;\r\n  font-size: 2rem bolder;\r\n  border-bottom: 4px solid #ccc;\r\n  margin: 0 0 1rem 0;\r\n}\r\n\r\n#app button {\r\n  display: inline-block;\r\n  margin: 5px;\r\n  font: inherit;\r\n  cursor: pointer;\r\n  border: 1px solid black;\r\n  background-color: DodgerBlue;\r\n  color: white;\r\n  padding: 0.05rem 1rem;\r\n  box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.26);\r\n}\r\n\r\n#app button:hover,\r\n#app button:active {\r\n  background-color: #c92858;\r\n  border-color: #ec3169;\r\n  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.26);\r\n}\r\n\r\n#app input {\r\n  font: inherit;\r\n  padding: 0.15rem;\r\n}\r\n#app label {\r\n  font-weight: bold;\r\n  margin-right: 1rem;\r\n  width: 7rem;\r\n  display: inline-block;\r\n}\r\n#app form div {\r\n  margin: 1rem 0;\r\n}\r\n</style>"],"sourceRoot":""}]}