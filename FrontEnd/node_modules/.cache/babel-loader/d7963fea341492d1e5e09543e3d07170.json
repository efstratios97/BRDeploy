<form class=\"overflow-auto\">\r\n              <MultiSelect\r\n                v-model=\"selected_apps_kpis\"\r\n                :options=\"apps\"\r\n                :filter=\"true\"\r\n                optionLabel=\"app\"\r\n                placeholder=\"Select Application\"\r\n                filterPlaceholder=\"Find an Application\"\r\n              />\r\n            </form>\r\n          </template>\r\n        </Card>\r\n        <Card class=\"component-card\">\r\n          <template v-slot:title> KPI </template>\r\n          <template v-slot:subtitle>\r\n            Select KPIs to perform KPI Analysis\r\n          </template>\r\n          <template v-slot:content>\r\n            <form class=\"overflow-auto\">\r\n              <MultiSelect\r\n                v-model=\"selected_kpis\"\r\n                :options=\"kpis\"\r\n                :filter=\"true\"\r\n                optionLabel=\"kpi.name\"\r\n                placeholder=\"Select KPI\"\r\n                filterPlaceholder=\"Find a KPI\"\r\n              />\r\n            </form>\r\n          </template>\r\n        </Card>\r\n      </div>\r\n      <br />\r\n      <div class=\"p-grid\">\r\n        <div class=\"p-col-4 p-offset-4\">\r\n          <div class=\"p-fluid\">\r\n            <Button\r\n              label=\"Apply\"\r\n              @click=\"render_kpis()\"\r\n              icon=\"pi pi-check\"\r\n              iconPos=\"right\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"container-...\" v-if=\"triggered_render_graph_kpi\">\r\n      <h1>Rendering KPIs...</h1>\r\n      <h3>Please wait...</h3>\r\n      <ProgressBar mode=\"indeterminate\" />\r\n    </div>\r\n    <div class=\"container-...\" v-if=\"rendered_graph_kpis\">\r\n      <div class=\"row row-cols-auto auto-cols-adj\">\r\n        <div v-for=\"data_point in data\" :key=\"data_point.app\">\r\n          <Card class=\"component-card\">\r\n            <template v-slot:content>\r\n              <div class=\"p-fluid\">\r\n                <kpi-gauge\r\n                  :id=\"data_point.app\"\r\n                  :data=\"data_point.data\"\r\n                  :caption=\"data_point.app\"\r\n                  :subCaption=\"subCaption\"\r\n                  :threshold=\"data_point.threshold\"\r\n                ></kpi-gauge>\r\n              </div>\r\n            </template>\r\n          </Card>\r\n        </div>\r\n        <Card class=\"component-card\">\r\n          <template v-slot:content>\r\n            <div class=\"p-fluid\">\r\n              <div v-for=\"selected_kpi in kpis\" :key=\"selected_kpi\">\r\n                <show-applicability-kpi\r\n                  :id=\"selected_kpi.kpi.name\"\r\n                  :selected_dataset_id=\"selected_dataset_id\"\r\n                  :selected_dataset_label=\"selected_dataset_label\"\r\n                  :selected_kpi=\"selected_kpi\"\r\n                ></show-applicability-kpi>\r\n              </div>\r\n            </div>\r\n          </template>\r\n        </Card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport KPIApplicabilityGauge from \"../Plots/KPIApplicabilityGauge.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"kpi-applicability-gauge\": KPIApplicabilityGauge,\r\n  },\r\n  props: [\"selected_dataset_id\", \"selected_dataset_label\"],\r\n  data() {\r\n    return {\r\n      selected_apps_kpis: \"\",\r\n      data: [],\r\n      rendered_graph_kpis: false,\r\n      subCaption: \"\",\r\n      triggered_render_graph_kpi: false,\r\n    };\r\n  },\r\n  methods: {\r\n    render_kpis() {\r\n      this.triggered_render_graph_kpi = true;\r\n      this.rendered_graph_kpis = false;\r\n      this.data = [];\r\n      this.$toast.add({\r\n        severity: \"info\",\r\n        summary: \"Rendering Aspect Visualization initiated...\",\r\n        detail: \"This make take some time.. Please wait\",\r\n        life: 15000,\r\n      });\r\n      var formData = new FormData();\r\n      formData.append(\"apps\", JSON.stringify(this.selected_apps_kpis));\r\n      formData.append(\"kpis\", JSON.stringify(this.selected_kpis));\r\n      this.$axios\r\n        .post(\r\n          \"/calculate_kpis/\" +\r\n            this.selected_dataset_id +\r\n            \"/\" +\r\n            this.selected_dataset_label,\r\n          formData\r\n        )\r\n        .then((res) => {\r\n          for (let index = 0; index < this.selected_apps_kpis.length; index++) {\r\n            var data_tmp = 0;\r\n            var threshold_tmp = 0;\r\n            for (\r\n              let index_2 = 0;\r\n              index_2 < res.data[this.selected_apps_kpis[index].app].length;\r\n              index_2++\r\n            ) {\r\n              data_tmp =\r\n                res.data[this.selected_apps_kpis[index].app][index_2].result;\r\n              threshold_tmp =\r\n                res.data[this.selected_apps_kpis[index].app][index_2].threshold;\r\n            }\r\n            this.data.push({\r\n              app: this.selected_apps_kpis[index].app,\r\n              data: data_tmp,\r\n              threshold: threshold_tmp,\r\n            });\r\n          }\r\n          this.subCaption = \"KPI Analysis per Application\";\r\n          this.triggered_render_graph_kpi = false;\r\n          this.rendered_graph_kpis = true;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}