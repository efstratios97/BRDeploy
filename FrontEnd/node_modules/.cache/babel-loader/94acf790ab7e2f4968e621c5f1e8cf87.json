RH,EAQY;AACjB,UAAM,WAAU,GAAI,OAAO,CAAC,KAAR,CAAc,GAAd,CAApB;AACA,aAAO,WAAP;AACD,KAXM;AAYP,IAAA,aAZO,2BAYS;AAAA;;AACd,WAAK,KAAL,CAAW,GAAX,CAAe,qCAAf,EAAsD,IAAtD,CAA2D,UAAC,GAAD,EAAS;AAClE,QAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAJ,CAAS,IAArB;AACA,QAAA,MAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,IAAJ,CAAS,IAA1B;AACD,OAHD;AAID;AAjBM;AANI,CAAf","sourcesContent":["<template>\r\n  <div class=\"table-responsive\">\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\">ID</th>\r\n          <th scope=\"col\">Name</th>\r\n          <th scope=\"col\">Description</th>\r\n          <th scope=\"col\">Supporting Datasets</th>\r\n          <th scope=\"col\">Supporting Headers</th>\r\n          <th scope=\"col\">Supporting Cleansing Operations</th>\r\n          <th scope=\"col\">User Operations</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"cleanser in cleansers\" :key=\"cleanser.cleanser_id\">\r\n          <td>{{ cleanser.cleanser_id }}</td>\r\n          <td>{{ cleanser.name }}</td>\r\n          <td>{{ cleanser.description }}</td>\r\n          <td>\r\n            <div class=\"dropdown\">\r\n              <button\r\n                class=\"btn btn-secondary dropdown-toggle\"\r\n                type=\"button\"\r\n                id=\"dropdownMenuButton1\"\r\n                data-bs-toggle=\"dropdown\"\r\n                aria-expanded=\"false\"\r\n              >\r\n                See Supported Datasets\r\n              </button>\r\n              <ul class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton1\">\r\n                <li\r\n                  v-for=\"dataset in this.strToList(cleanser.datasets)\"\r\n                  :key=\"dataset\"\r\n                >\r\n                  <p class=\"dropdown-item\">{{ dataset }}</p>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div class=\"dropdown\">\r\n              <button\r\n                class=\"btn btn-secondary dropdown-toggle\"\r\n                type=\"button\"\r\n                id=\"dropdownMenuButton1\"\r\n                data-bs-toggle=\"dropdown\"\r\n                aria-expanded=\"false\"\r\n              >\r\n                See Supported Headers\r\n              </button>\r\n              <ul class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton1\">\r\n                <li\r\n                  v-for=\"header in this.strToList(cleanser.header_list)\"\r\n                  :key=\"header\"\r\n                >\r\n                  <p class=\"dropdown-item\">{{ header }}</p>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div class=\"dropdown\">\r\n              <button\r\n                class=\"btn btn-secondary dropdown-toggle\"\r\n                type=\"button\"\r\n                id=\"dropdownMenuButton1\"\r\n                data-bs-toggle=\"dropdown\"\r\n                aria-expanded=\"false\"\r\n              >\r\n                See Supported Operations\r\n              </button>\r\n              <ul class=\"dropdown-menu\" aria-labelledby=\"dropdownMenuButton1\">\r\n                <li\r\n                  v-for=\"operations in this.strToList(\r\n                    cleanser.cleanser_operation_types\r\n                  )\"\r\n                  :key=\"operations\"\r\n                >\r\n                  <p class=\"dropdown-item\">{{ operations }}</p>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <button\r\n              v-on:click=\"deleteCleanser(cleanser.cleanser_id)\"\r\n              class=\"btn btn-secondary\"\r\n            >\r\n              <i class=\"fa fa-trash\"></i>\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      cleansers: this.listCleansers(),\r\n    };\r\n  },\r\n  methods: {\r\n    deleteCleanser(cleanser_id) {\r\n      this.axios\r\n        .delete(\"http://127.0.0.1:8081/delete_cleanser/\" + cleanser_id)\r\n        .then(() => {\r\n          this.listCleansers();\r\n        });\r\n    },\r\n    strToList(strings) {\r\n      const list_string = strings.split(\",\");\r\n      return list_string;\r\n    },\r\n    listCleansers() {\r\n      this.axios.get(\"http://127.0.0.1:8081/get_cleansers\").then((res) => {\r\n        console.log(res.data.data);\r\n        this.cleansers = res.data.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: dodgerblue;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"],"sourceRoot":""}]}