       <div class=\"col-...\">\r\n                <plotter-simple-statistics\r\n                  :selected_dataset=\"selected_dataset\"\r\n                  :key=\"componentKey\"\r\n                ></plotter-simple-statistics>\r\n              </div>\r\n            </div>\r\n            <br />\r\n            <div class=\"row justify-content-center pb-5 pt-2\">\r\n              <div class=\"col-8\">\r\n                <h1 class=\"h1_sub\" v-if=\"showFurtherSimpleStatistics\">\r\n                  Remove Additional Statistics Analyzer\r\n                </h1>\r\n                <h1 class=\"h1_sub\" v-else>\r\n                  Add Additional Statistics Analyzer\r\n                </h1>\r\n                <div class=\"d-grid\">\r\n                  <button\r\n                    class=\"btn btn-secondary\"\r\n                    type=\"button\"\r\n                    @click=\"toggleShowFurtherSimpleStatistics\"\r\n                  >\r\n                    <span class=\"btn-label\"\r\n                      ><b-icon-dash-square-fill\r\n                        v-if=\"\r\n                          showFurtherSimpleStatistics\r\n                        \" /><b-icon-plus-square-fill v-else\r\n                    /></span>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div\r\n            class=\"row\"\r\n            v-if=\"datasetSelected && showFurtherSimpleStatistics\"\r\n          >\r\n            <div class=\"col\">\r\n              <transition class=\"modal-animation\">\r\n                <plotter-simple-statistics\r\n                  :selected_dataset=\"selected_dataset\"\r\n                  :key=\"componentKey\"\r\n                ></plotter-simple-statistics>\r\n              </transition>\r\n            </div>\r\n            <div class=\"col\">\r\n              <transition class=\"modal-animation\">\r\n                <plotter-simple-statistics\r\n                  :selected_dataset=\"selected_dataset\"\r\n                  :key=\"componentKey\"\r\n                ></plotter-simple-statistics>\r\n              </transition>\r\n            </div>\r\n          </div>\r\n          <br />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BubblePlotter from \"../components/PlotterBubble.vue\";\r\nimport PlotterSimpleStatistics from \"../components/PlotterSimpleStatistics.vue\";\r\nimport SelectorAnalyzerDataset from \"../components/SelectorAnalyzerDataset.vue\";\r\nimport Modal from \"../components/Modal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"widgets-bubble-plotter\": BubblePlotter,\r\n    \"modal-view\": Modal,\r\n    \"selector-analyzer-dataset\": SelectorAnalyzerDataset,\r\n    \"plotter-simple-statistics\": PlotterSimpleStatistics,\r\n  },\r\n  data() {\r\n    this.autenticateSession();\r\n    return {\r\n      showAnalyzerSelector: false,\r\n      showFurtherHeatMaps: false,\r\n      showFurtherSimpleStatistics: false,\r\n      selected_dataset: \"\",\r\n      datasetSelected: false,\r\n      componentKey: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleShowFurtherSimpleStatistics() {\r\n      this.showFurtherSimpleStatistics = !this.showFurtherSimpleStatistics;\r\n    },\r\n    toggleShowFurtherHeatMaps() {\r\n      this.showFurtherHeatMaps = !this.showFurtherHeatMaps;\r\n    },\r\n    toggleShowAnalyzerSelector() {\r\n      this.showAnalyzerSelector = !this.showAnalyzerSelector;\r\n    },\r\n    updateSelectedDataset(value) {\r\n      console.log(value);\r\n      this.selected_dataset = value;\r\n      this.datasetSelected = true;\r\n      this.componentKey++;\r\n      this.toggleShowAnalyzerSelector();\r\n    },\r\n    autenticateSession() {\r\n      if (localStorage.loggedUser && localStorage.token) {\r\n        console.log(localStorage.token);\r\n        console.log(localStorage.loggedUser);\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/user/validatetoken?token=\" +\r\n              localStorage.token\r\n          )\r\n          .then(() => {\r\n            this.selected_user = localStorage.loggedUser;\r\n            this.axios\r\n              .get(\"http://127.0.0.1:8081/user/\" + this.selected_user)\r\n              .then((res) => {\r\n                this.$store.state.loggedUser = res.data;\r\n                console.log(this.$store.state.loggedUser);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$router.push(\"/\");\r\n          });\r\n      } else {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.bg-img {\r\n  background-image: url(\"~@/assets/DataAnalyzerWallpaper2.png\");\r\n}\r\n.bokeh-graph {\r\n  resize: both;\r\n}\r\n</style>"]}]}