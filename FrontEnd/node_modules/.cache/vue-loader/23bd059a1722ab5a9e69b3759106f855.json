his.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        console.log(res.data);\r\n        this.users = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbutton {\r\n  color: white;\r\n  background: dodgerblue;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"]}]}