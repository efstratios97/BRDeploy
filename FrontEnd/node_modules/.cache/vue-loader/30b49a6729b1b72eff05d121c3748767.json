C,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;EACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACpB","file":"C:/Users/Strat/Desktop/ProjectAthena/AthenaSRC/FrontEnd/project_athena/project-athena/src/components/AddUser.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <form>\r\n    <div class=\"mb-3\">\r\n      <label for=\"DatasetName\" class=\"form-label\">\r\n        <b-icon-archive-fill /> Name:</label\r\n      >\r\n      <input\r\n        class=\"input100\"\r\n        id=\"DatasetName\"\r\n        type=\"text\"\r\n        v-model=\"name\"\r\n        placeholder=\"Data Set Name\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label for=\"DatasetName\" class=\"form-label\" id=\"description\">\r\n        <b-icon-pencil-fill /> Description:\r\n      </label>\r\n      <textarea\r\n        class=\"input100\"\r\n        v-model=\"description\"\r\n        placeholder=\"Description\"\r\n        id=\"description\"\r\n      ></textarea>\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label for=\"cleaned\"> <b-icon-check-circle-fill /> Cleaned</label>\r\n      <input class=\"input200\" type=\"checkbox\" v-model=\"cleaned\" id=\"cleaned\" />\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label for=\"storage\"> <b-icon-cloud-check-fill /> Stored in Cloud </label>\r\n      <input\r\n        class=\"input200\"\r\n        type=\"checkbox\"\r\n        :storageType=\"storageType\"\r\n        id=\"storage\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label> <b-icon-shield-lock-fill /> Users with Access</label>\r\n      <select v-model=\"selected_users\" multiple>\r\n        <option\r\n          v-for=\"user in users\"\r\n          v-bind:value=\"user.email\"\r\n          :key=\"user.email\"\r\n        >\r\n          {{ user.email }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label> <b-icon-shield-lock-fill /> Departments with Access</label>\r\n      <select v-model=\"selected_departments\" multiple>\r\n        <option\r\n          v-for=\"department in departments\"\r\n          v-bind:value=\"department.name\"\r\n          :key=\"department.name\"\r\n        >\r\n          {{ department.name }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <div class=\"mb-3\">\r\n      <label for=\"formFile\" class=\"form-label\">File Input</label>\r\n      <input class=\"form-control\" type=\"file\" id=\"formFile\" :file=\"file\" />\r\n    </div>\r\n\r\n    <button class=\"btn-form\" @click=\"createDataSet\">\r\n      <b-icon-upload />\r\n      <b> Add Data Set</b>\r\n    </button>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      cleand: \"\",\r\n      description: \"\",\r\n      storageType: \"\",\r\n      file: \"\",\r\n      users: this.getUsersOptions(),\r\n      departments: this.getDepartmentsOptions(),\r\n      selected_users: \"\",\r\n      selected_departments: \"\",\r\n      dataset: {},\r\n    };\r\n  },\r\n  methods: {\r\n    getUsersOptions() {\r\n      this.users = this.axios.get(\"http://127.0.0.1:8081/users\").then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n    getDepartmentsOptions() {\r\n      this.users = this.axios\r\n        .get(\"http://127.0.0.1:8081/departments\")\r\n        .then((res) => {\r\n          this.departments = res.data;\r\n        });\r\n    },\r\n    createDataSet() {\r\n      this.dataset = {\r\n        name: this.name,\r\n        cleaned: this.cleaned,\r\n        data: this.file,\r\n        access_user_list: this.selected_users[0],\r\n        access_business_unit_list: this.selected_departments[0],\r\n        description: this.description,\r\n        storage_type: this.storageType,\r\n      };\r\n      const jsonString = JSON.stringify(this.dataset);\r\n      console.log(jsonString);\r\n      this.axios\r\n        .post(\r\n          \"http://127.0.0.1:8081/create_dataset?uid=\" + localStorage.loggedUser,\r\n          jsonString\r\n        )\r\n        .then((res) => {\r\n          console.log(res.data);\r\n        });\r\n    },\r\n\r\n    onUploadFile(event) {\r\n      const file = event.target.files[0];\r\n      console.log(file);\r\n      this.axios\r\n        .post(\"http://127.0.0.1:8081/upload_dataset\", file, {\r\n          headers: {\r\n            \"Content-Type\": \"multipart/form-data\",\r\n          },\r\n        })\r\n        .then(console.log)\r\n        .catch(console.error);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\nlabel {\r\n  display: block;\r\n}\r\n\r\n.btn-form {\r\n  color: white;\r\n  background: dodgerblue;\r\n  border: 0px solid blue;\r\n  border-radius: 2px;\r\n}\r\n</style>"]}]}