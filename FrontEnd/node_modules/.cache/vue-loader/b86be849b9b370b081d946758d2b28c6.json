 from \"../components/DataSetTable.vue\";\r\nimport AddDataSet from \"../components/AddDataSet.vue\";\r\nexport default {\r\n  components: { \"data-set-table\": DataSetTable, \"add-data-set\": AddDataSet },\r\n  data() {\r\n    this.autenticateSession();\r\n    return {\r\n      datasets: this.listDatasets(),\r\n    };\r\n  },\r\n  methods: {\r\n    listDatasets() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8081/get_datasets/\" + localStorage.loggedUser)\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          this.datasets = res.data.data;\r\n        });\r\n    },\r\n    autenticateSession() {\r\n      if (localStorage.loggedUser && localStorage.token) {\r\n        console.log(localStorage.token);\r\n        console.log(localStorage.loggedUser);\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/user/validatetoken?token=\" +\r\n              localStorage.token\r\n          )\r\n          .then(() => {\r\n            this.selected_user = localStorage.loggedUser;\r\n            this.axios\r\n              .get(\"http://127.0.0.1:8081/user/\" + this.selected_user)\r\n              .then((res) => {\r\n                this.$store.state.loggedUser = res.data;\r\n                console.log(this.$store.state.loggedUser);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$router.push(\"/\");\r\n          });\r\n      } else {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\");\r\n\r\n.pt-tablecell {\r\n  display: table-cell;\r\n  vertical-align: text-top;\r\n}\r\n\r\n.serviceBox {\r\n  background: #1e2530;\r\n  font-family: \"Poppins\", sans-serif;\r\n  text-align: center;\r\n  border-radius: 50px;\r\n  border: 5px solid dodgerblue;\r\n  width: auto;\r\n  overflow: hidden;\r\n  top: 10px;\r\n  position: relative;\r\n  display: -webkit-box;\r\n  display: -webkit-flex;\r\n  display: -moz-box;\r\n  display: -ms-flexbox;\r\n  padding: 110px 130px 157px 148px;\r\n}\r\n\r\n.serviceBox .service-icon {\r\n  color: DodgerBlue;\r\n  font-size: 50px;\r\n  line-height: 50px;\r\n  margin: 1px 1 25px;\r\n  transition: all 0.3s ease 0s;\r\n}\r\n\r\n.serviceBox:hover .service-icon {\r\n  transform: rotate(360deg);\r\n}\r\n\r\nhr {\r\n  background: dodgerblue;\r\n  height: 20px;\r\n  border: none;\r\n}\r\n\r\n.overlay {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n@media only screen and (max-width: 990px) {\r\n  .serviceBox {\r\n    margin: 0 0 30px;\r\n  }\r\n}\r\n</style>"]}]}