sourcesContent":[" <template>\r\n  <div>\r\n    <div class=\"banner-head banner-image p-shadow-14\"></div>\r\n    <div class=\"page-background\">\r\n      <div class=\"container-xxl main-page p-shadow-14\">\r\n        <h1 style=\"text-align: left; font-size: 38px\">User Manager</h1>\r\n        <br />\r\n        <br />\r\n        <TabView class=\"tabview-custom\" ref=\"tabview4\">\r\n          <TabPanel\r\n            ><template #header>\r\n              <i class=\"pi pi-users\" style=\"font-size: 13p; margin: 1px\"></i>\r\n              <span><h4>Users</h4></span>\r\n            </template>\r\n            <user-table :key=\"componentKey\"></user-table>\r\n          </TabPanel>\r\n          <TabPanel\r\n            ><template #header>\r\n              <i\r\n                class=\"pi pi-briefcase\"\r\n                style=\"font-size: 13px; margin: 1px\"\r\n              ></i>\r\n              <span><h4>Departments</h4></span>\r\n            </template>\r\n            <div class=\"container-sm justify-content-start\">\r\n              <div class=\"row justify-content-start\">\r\n                <div class=\"col\">\r\n                  <dep-table :key=\"componentKey\"></dep-table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </TabPanel>\r\n          <TabPanel\r\n            ><template #header>\r\n              <i\r\n                class=\"pi pi-briefcase\"\r\n                style=\"font-size: 13px; margin: 1px\"\r\n              ></i>\r\n              <span><h4>UserManager Operation</h4></span>\r\n            </template>\r\n          </TabPanel>\r\n        </TabView>\r\n        <div class=\"row\">\r\n          <div class=\"col-8\">\r\n            <h1 class=\"h1_sub\">Create User</h1>\r\n            <div class=\"d-grid\">\r\n              <button\r\n                class=\"btn btn-primary\"\r\n                type=\"button\"\r\n                @click=\"toggleShowAddUser\"\r\n              >\r\n                <span class=\"btn-label\"><b-icon-person-plus-fill /></span>\r\n              </button>\r\n            </div>\r\n            <transition class=\"modal-animation\">\r\n              <modal-view v-if=\"showAddUser\" @close=\"toggleShowAddUser\">\r\n                <template v-slot:header>ADD USER</template>\r\n                <template v-slot:body>\r\n                  <add-user @close=\"refreshData\"> </add-user> </template\r\n              ></modal-view>\r\n            </transition>\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <h1 class=\"h1_sub\">New Department</h1>\r\n            <div class=\"d-grid\">\r\n              <button\r\n                class=\"btn btn-primary\"\r\n                type=\"button\"\r\n                @click=\"toggleShowAddDep\"\r\n              >\r\n                <span class=\"btn-label\"><b-icon-plus-circle-fill /></span>\r\n              </button>\r\n            </div>\r\n            <transition class=\"modal-animation\">\r\n              <modal-view v-if=\"showAddDep\" @close=\"toggleShowAddDep\">\r\n                <template v-slot:header>ADD Department</template>\r\n                <template v-slot:body>\r\n                  <add-dep @close=\"refreshData\"> </add-dep> </template\r\n              ></modal-view>\r\n            </transition>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <br />\r\n      <br />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserTable from \"../components/UserTable.vue\";\r\nimport TableDepartments from \"../components/TableDepartments.vue\";\r\nimport AddUser from \"../components/AddUser.vue\";\r\nimport AddDepartment from \"../components/AddDepartment.vue\";\r\nimport Modal from \"../components/Modal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    \"user-table\": UserTable,\r\n    \"add-user\": AddUser,\r\n    \"add-dep\": AddDepartment,\r\n    \"modal-view\": Modal,\r\n    \"dep-table\": TableDepartments,\r\n  },\r\n  data() {\r\n    this.autenticateSession();\r\n    return {\r\n      showAddUser: false,\r\n      showAddDep: false,\r\n      componentKey: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleShowAddUser() {\r\n      this.showAddUser = !this.showAddUser;\r\n    },\r\n    toggleShowAddDep() {\r\n      this.showAddDep = !this.showAddDep;\r\n    },\r\n    refreshData() {\r\n      this.componentKey += 1;\r\n      if (this.showAddUser === true) {\r\n        this.toggleShowAddUser();\r\n      }\r\n      if (this.showAddDep === true) {\r\n        this.toggleShowAddDep();\r\n      }\r\n    },\r\n    autenticateSession() {\r\n      if (localStorage.loggedUser && localStorage.token) {\r\n        console.log(localStorage.token);\r\n        console.log(localStorage.loggedUser);\r\n        this.axios\r\n          .get(\r\n            \"http://127.0.0.1:8081/user/validatetoken?token=\" +\r\n              localStorage.token\r\n          )\r\n          .then(() => {\r\n            this.selected_user = localStorage.loggedUser;\r\n            this.axios\r\n              .get(\"http://127.0.0.1:8081/user/\" + this.selected_user)\r\n              .then((res) => {\r\n                this.$store.state.loggedUser = res.data;\r\n                console.log(this.$store.state.loggedUser);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$router.push(\"/\");\r\n          });\r\n      } else {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.container-xxl {\r\n  min-width: calc(95%);\r\n}\r\n</style>\r\n"]}]}